{var stateObj={'url':pr_url,'title':pr_title,'slide':pr_next_item};document.title=pr_title;window.history.pushState(stateObj,document.title,pr_url);if((typeof gtag=='function')||(typeof GA_TRACKING_ID=='string'))
{gtag('event','Presentation',{'event_category':'Interaction','event_label':'NextItem','value':pr_next_item});gtag('config',GA_TRACKING_ID,{'page_location':pr_url,'page_path':location.pathname+location.search+location.hash,'page_title':pr_title});}
else
if(typeof ga=='function')
{ga('send',{hitType:'event',eventCategory:'Interaction',eventAction:'Presentation',eventLabel:'NextItem',eventValue:pr_next_item});ga('send','pageview',{'location':pr_url,'title':pr_title,'page':location.pathname+location.search+location.hash});}}
if(pr_inViewport!=1)
scrollWin('#'+pr_object+' .art-presentation-item-'+pr_next_item);$('#'+pr_object).css('min-height','auto');});});event.preventDefault();event.stopPropagation();return false;});});$(this).addClass('binded');}});}}
function initCommunity()
{if($('#header-soc-area').length==1)
{var comjs=document.createElement('script');comjs.id='js_community';comjs.src='/_js/community.js';comjs.type='text/javascript';comjs.async=true;if(comjs.readyState)
