function saveSharedCount(sender)
{var id=getTargeData('id'),type=getTargeData('type');if((id>0)&&(type>0))
{$.ajax({type:'POST',url:'/hits.php',data:{'id':''+id,'type':''+type,'to':''+$(sender).data('to'),'event':'share','action':'set'},dataType:'json',success:function(oJsonObject){if(oJsonObject.count&&(oJsonObject.count>0))
{$('.share-button-icon').each(function(){if($('em',this).length==0)
$(this).append('<em class="added">'+oJsonObject.count+'</em>');else
$('em',this).text(oJsonObject.count);$('em',this).addClass('added');});$('.art-details-share').css('margin-right',$('.art-details-share .share-button-icon').width());}
$('.header-art-share').html(((oJsonObject.count>0)?'<span>'+oJsonObject.count+'</span>':'<span></span>'));$('.header-art-share').addClass('visible');if((typeof gtag=='function')||(typeof GA_TRACKING_ID=='string'))
{gtag('event','Share',{'event_category':'Interaction','event_label':''+$(sender).data('to'),'value':id});}
else
if(typeof ga=='function')
{ga('send',{hitType:'event',eventCategory:'Interaction',eventAction:'Share',eventLabel:''+$(sender).data('to'),eventValue:id});}},error:function(){}});}}
