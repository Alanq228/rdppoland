{if(!googletag.pubadsReady)
{if(show_log)
console.log('genesisAds internal','initGenesisAds','!pubadsReady');initAdIndex++;if(initAdIndex>250)
{if(show_log)
console.log('genesisAds internal','initGenesisAds','!init exit',initAdIndex,'googletag = '+typeof(window.googletag),'apiReady = '+googletag.apiReady,'pubadsReady = '+googletag.pubadsReady);window.clearInterval(initAdInterval);return false;}}
else
{if(show_log)
