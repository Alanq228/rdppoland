{if(visible)
$(slot_obj).parent('div').removeClass('hidden');else
$(slot_obj).parent('div').addClass('hidden');}
else
if($(slot_obj).parent('div').hasClass('page-rectangle-inner'))
{if(visible)
{$(slot_obj).parent('div.page-rectangle-inner').removeClass('empty-rectangle');if($(slot_obj).parents('div.page-rectangle').length==1)
$(slot_obj).parents('div.page-rectangle').removeClass('hidden');}
else
{$(slot_obj).parent('div.page-rectangle-inner').addClass('empty-rectangle');if($(slot_obj).parents('div.page-rectangle').length==1)
$(slot_obj).parents('div.page-rectangle').addClass('hidden');}}
else
