function readGenesisAdsConfig()
{$(document).ready(function(){if($('div[data-genesis="ads-config"]').length>0)
{let _conf=$('div[data-genesis="ads-config"]').first();genAdsConfig={dfp:((''+$(_conf).data('dfp')).toLowerCase()!='false'),optad360:((''+$(_conf).data('optad360')).toLowerCase()!='false')}
if(genAdsConfig.optad360)
{if($.trim($(_conf).data('optad360-src'))!='')
{genAdsConfig.optad360_src=$.trim($(_conf).data('optad360-src'));genAdsConfig.optad360_release=(($.trim($(_conf).data('optad360-release'))!='')?$(_conf).data('optad360-release').toLowerCase():'always');}}}
if(show_log)
console.log('genesisAds internal','Config',genAdsConfig,'EmbedDomain',embedDomain);if(genAdsConfig.dfp)
initGenesisAdsWithDFP();else
runGenesisAds();});}
