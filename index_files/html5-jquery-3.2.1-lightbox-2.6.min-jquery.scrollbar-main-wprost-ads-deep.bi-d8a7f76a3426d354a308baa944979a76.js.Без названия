{var id=getTargeData('id'),type=getTargeData('type');if((id>0)&&(type>0))
{$.ajax({type:'POST',url:'/hits.php',data:{'id':''+id,'type':''+type,'event':'share','action':'get'},dataType:'json',success:function(oJsonObject){if(oJsonObject.count&&(oJsonObject.count>0))
{$('.share-button-icon').each(function(){if($('em',this).length==0)
