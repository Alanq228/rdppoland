{var date=new Date(),ts=date.toLocaleTimeString(),ds=date.toISOString(),date_now=ds.substr(0,10)+' '+ts.substr(0,5),show_info=((date_now>'2019-10-11 21:00')&&(date_now<'2019-10-13 21:00'));if(show_info)
{$('.page-box.page-box-wybory-2019, .page-box.page-box-wybory-2019-top').addClass('page-box-wybory-2019-silence');$('.page-header-wybory-2019').addClass('page-header-wybory-2019-silence');}
