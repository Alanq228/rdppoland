{if(show_info&&($('.page-box.page-box-wybory-2019-top li.box-list-item-empty').length>0)&&($('.page-box.page-box-wybory-2019-top .page-box-wybory-2019-info').length>0))
{$('.page-box.page-box-wybory-2019-top .page-box-wybory-2019-info').slideDown(600);$('.page-box.page-box-wybory-2019-top').addClass('page-box-wybory-2019-top-and-info');}
