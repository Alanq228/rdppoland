function ad_DFP_CreateDynamicSlotByIndex(index,type,placeholder,targeting)
{if(show_log)
console.log('genesisAds DFP','ad_DFP_CreateDynamicSlotByIndex',index,type,placeholder);if((window.googletag&&googletag.apiReady)&&(typeof DFP_DynamicSlots=='object')&&(typeof DFP_DynamicSlots[index]=='object')&&(typeof DFP_DynamicSlots[index][type]=='object')&&(!(!(document.getElementById(placeholder)))))
{if(show_log)
console.log('genesisAds DFP','ad_DFP_CreateDynamicSlotByIndex',DFP_DynamicSlots[index][type],(typeof DFP_DynamicSlots[index][type].size),DFP_DynamicSlots[index][type].size.length,(((typeof DFP_DynamicSlots[index][type].size=='object')||(typeof DFP_DynamicSlots[index][type].size=='array'))&&(DFP_DynamicSlots[index][type].size.length>0)));if(typeof DFP_DynamicSlots[index][type]=='object')
{var ph_width=$('#'+placeholder).width(),sizes=[],width=1,height=1;if(((typeof DFP_DynamicSlots[index][type].size=='object')||(typeof DFP_DynamicSlots[index][type].size=='array'))&&(DFP_DynamicSlots[index][type].size.length>0))
{var w,h=0;for(var i=0;i<DFP_DynamicSlots[index][type].size.length;i++)
{w=parseInt($.trim(DFP_DynamicSlots[index][type].size[i][0]));h=parseInt($.trim(DFP_DynamicSlots[index][type].size[i][1]));if(w<=ph_width)
{sizes.push([w,h]);if(w>width)
width=w;if(h>height)
height=h;}}
if(sizes.length>0)
{$('#'+placeholder).css({'display':'inline-block','min-height':'1px','min-width':'1px','max-height':height,'max-width':width});if(show_log)
console.log('genesisAds DFP','ad_DFP_CreateDynamicSlotByIndex',placeholder,width,height,ph_width,sizes);var new_slot={'id':placeholder,'sizes':sizes,'width':width,'height':height,'ph_width':ph_width,'can_refresh':(DFP_DynamicSlots[index][type].refresh||(DFP_DynamicSlots[index][type].refresh=='true')),'type_genesis':false,'type_dfp':true,'slot':DFP_DynamicSlots[index][type].adUnitPath,'dfp_slot':ad_DFP_CreateDynamicSlot(placeholder,DFP_DynamicSlots[index][type].adUnitPath,sizes,false,'load'),}
genAdSlots.push(new_slot);if(show_log)
console.log('genesisAds DFP','ad_DFP_CreateDynamicSlotByIndex new_slot',new_slot);if(typeof new_slot.dfp_slot=='object')
{runRefreshAdsInterval();$('#'+placeholder).attr('data-ad','genesis').data('ad','genesis');$('#'+placeholder).attr('data-type','doubleclick').data('type','doubleclick');$('#'+placeholder).attr('data-assigned',1).data('assigned',1);return new_slot;}
else
showHideGenesisSlotParentArea(placeholder,false);}
else
{if(show_log)
console.log('genesisAds DFP','ad_DFP_CreateDynamicSlotByIndex','!sizes');}}
else
return false;}
else
return false;}
else
return false;}
function ad_DFP_CreateDynamicSlot(placeholder,adUnitPath,adSize,targeting,release)
{var placeholder_object=document.getElementById(placeholder);if(show_log)
console.log('genesisAds DFP','CreateDynamicSlot','googletag && pubadsReady = '+(window.googletag&&googletag.pubadsReady),(typeof adUnitPath)+' = '+(typeof adUnitPath=='string'),(typeof adSize)+' = '+((typeof adSize=='object')||(typeof adSize=='array')),placeholder+' = '+placeholder_object);if((window.googletag&&googletag.pubadsReady)&&(typeof adUnitPath=='string')&&((typeof adSize=='object')||(typeof adSize=='array'))&&(!(!(placeholder_object))))
{$('#'+placeholder).attr('data-dfp-sizes',adSize.join(','));var dfp_slot=false,in_viewport=isElementInVerticalViewport(document.getElementById(placeholder));if(show_log)
console.log('genesisAds DFP','inViewport',placeholder,in_viewport);if((release=='load')||in_viewport)
{dfp_slot=ad_DFP_defineDisplaySlot(placeholder,adUnitPath,adSize,targeting);if(typeof dfp_slot!='object')
showHideGenesisSlotParentArea(placeholder,false);}
else
{if(show_log)
{console.log('genesisAds DFP','Waiting',placeholder,$('#'+placeholder).length);}
dfp_slot=tempDFPObject;$('#'+placeholder).attr('data-dfp-state','wait').data('dfp-state','wait');$('#'+placeholder).addClass('slot-dynamic');}
return dfp_slot;}
else
return false;}
function ad_DFP_defineDisplaySlot(placeholder,adUnitPath,adSize,targeting)
{var dfp_slot=googletag.defineSlot(adUnitPath,adSize,placeholder);if(show_log)
console.log('genesisAds DFP','DefineSlot',dfp_slot,placeholder,adUnitPath,adSize);if(!(!dfp_slot))
{if(show_log)
console.log('genesisAds DFP','onLoad',placeholder);dfp_slot.addService(googletag.pubads());if(googletag.pubads().isInitialLoadDisabled())
{$('#'+placeholder).attr('data-dfp-isInitialLoadDisabled','true');googletag.pubads().refresh([dfp_slot],{changeCorrelator:false});if(show_log)
console.log('genesisAds DFP','pubads refresh',placeholder);}
else
{googletag.display(placeholder);if(show_log)
console.log('genesisAds DFP','display',placeholder);}
$('#'+placeholder).attr('data-dfp-state','display').data('dfp-state','display');$('#'+placeholder).addClass('slot-dynamic');return dfp_slot;}
else
return false;}
