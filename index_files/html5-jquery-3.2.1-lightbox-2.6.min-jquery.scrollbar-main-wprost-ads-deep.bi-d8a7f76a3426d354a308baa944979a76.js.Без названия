{if(keeped)
{var px=new Image;px.src='/_i/pusty.png';px.onload=function(){$.ajax({type:'POST',url:'/hits.php',data:{'href':location.href,'type':100,'event':'display','action':'set','keeped':keeped,'referer':(pageReferrer?atob(pageReferrer):document.referer),'referrer':(pageReferrer?atob(pageReferrer):document.referrer)},dataType:'json',success:function(oJsonObject){}});}}
else
if(!sendH)
{sendH=true;hitsEvents['display_href_set']={'href':location.href,'type':100,'event':'display','action':'set','referer':(pageReferrer?atob(pageReferrer):document.referer),'referrer':(pageReferrer?atob(pageReferrer):document.referrer)};}}
function getTargeData(data)
{var value=0;if($('#element').data(data))
value=$('#element').data(data);else
if($('#element-landing-page').data(data))
value=$('#element-landing-page').data(data);else
if($('#gallery').data(data))
value=$('#gallery').data(data);else
if($('#quiz').data(data))
value=$('#quiz').data(data);else
if($('#profile').data(data))
value=$('#profile').data(data);return value;}
function expandHeaderBar(sender)
{var bar=$(sender).data('header');$('.header-button').removeClass('selected');if($('#header').hasClass('expanded'))
{if($('#header-'+bar).is(':visible'))
closeHeaderBars();else
{$('.header-button').removeClass('selected');$(sender).addClass('selected');$('.header-expand-bar').hide();$('#header-'+bar).show();if(bar=='search')
$('#q').focus();}}
else
{$('.header-expand-bar').hide();$(sender).addClass('selected');$('#header').addClass('expanded');$('#header-'+bar).slideDown(500,function(){if(bar=='search')
$('#q').focus();});}
if($('#header').hasClass('expanded'))
$('body').addClass('header-expanded');else
$('body').removeClass('header-expanded');}
function closeHeaderBars()
{hideHeaderMessage();$('#header').removeClass('expanded');$('.header-button').removeClass('selected');$('.header-expand-bar').slideUp();$('body').removeClass('header-expanded');}
function scrollWin(param,offset)
{if(!offset)
{if($('#header').is(':visible'))
offset=-80;if(($('#element-header').hasClass('pinned'))&&($('#element-header').is(':visible')))
offset=-140;}
if(offset=='header')
{offset=0-($('#header').outerHeight());}
else
if(offset=='headers')
{offset=-140;}
if(typeof param=="number")
$('html,body').animate({scrollTop:param});else
if(typeof param=="string")
{if($(param).length==1)
{var tpx=$(param).offset().top;if(offset)
tpx=tpx+offset;$('html,body').animate({scrollTop:tpx});}}}
function scrollWinTop(nogasend)
{$('html,body').animate({scrollTop:0});if((!nogasend)&&(typeof gtag=='function')||(typeof GA_TRACKING_ID=='string'))
{gtag('event','ScrollWindow',{'event_category':'Interaction','event_label':'Top'});}
else
if((!nogasend)&&(typeof ga=='function'))
{ga('send',{hitType:'event',eventCategory:'Interface',eventAction:'ScrollWindow',eventLabel:'Top'});}}
function scrollWinBottom()
{var tpx=$('body').height();$('html,body').animate({scrollTop:tpx});}
