{pnt=pnt+parseInt($('#element .article .art-authors').first().height());pnt=pnt+parseInt($('#element .article .art-authors').css('margin-top').replace('px',''));pnt=pnt+parseInt($('#element .article .art-authors').css('margin-bottom').replace('px',''));}
