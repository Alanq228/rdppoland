function scrollPage(event)
{scrlTop=$(window).scrollTop();scrlDirection=((scrlTop<old_scrlTop)?'up':'down');var wH=$(window).height(),wW=$(window).width(),hH=$('#header').height(),sZ=$('body').hasClass('scroll-0p'),phT=0,sTP=0;sTP=Math.round((scrlTop/wH)*100)
for(var i=0;i<=10;i++)
$('body').removeClass('scroll-'+(i*10)+'p');if(sTP<9)
$('body').addClass('scroll-0p');else
if(sTP<100)
{for(var i=0;i<=10;i++)
{if((i*10)>=sTP)
{$('body').addClass('scroll-'+(i*10)+'p');break;}}}
else
$('body').addClass('scroll-100p');if(scrlTop>0)
{$('#header').addClass('pinned');$('.header-message').addClass('pinned');if($('#element').length==1)
{var eT=$('#element').offset().top,eH=$('#element').outerHeight(),eB=(scrlTop+wH)-eT,eE=eB+$('#element').outerHeight(),pr=0;if((eB>0)&&(eB<eH))
{pr=parseFloat((eB*100)/eH);$('#element-header .progress').css({'width':pr+'%'});}
else
if((eB>0)&&(eB>=eH))
$('#element-header .progress').css({'width':'100%'});else
$('#element-header .progress').css({'width':0});}
if(wW<=800)
{if(scrlTop>300)
{if(scrlTop>old_scrlTop)
$('#header, #element-header').addClass('pinned-small');else
$('#header, #element-header').removeClass('pinned-small');}
else
$('#header, #element-header').removeClass('pinned-small');}
else
$('#header, #element-header').removeClass('pinned-small');}
else
{$('#header').removeClass('pinned').removeClass('pinned-small');$('.header-message').removeClass('pinned');$('#element-header .progress').css({'width':0});}
if(wW>1190)
{if(($('#element .left-column').length==1)&&($('#element .right-column').length==1)&&($('#element .right-column-fix.floating').length==1))
{var shO=0
shT=0,rfT=$('#element .right-column-fix.floating').offset().top,rfH=$('#element .right-column-fix.floating').outerHeight();riH=$('#element .right-column-fix.floating .right-column-fix-inner').outerHeight();if(($('#header').hasClass('pinned'))&&($('#header').is(':visible')))
shO=60;if(($('#element-header').hasClass('pinned'))&&($('#element-header').is(':visible')))
shO=120;shO=shO+20;if(rfT<=scrlTop+shO)
{$('#element .right-column-fix.floating').addClass('pinned');$('#element .right-column-fix.floating .right-column-fix-inner').css({'margin-top':shO});}
else
{$('#element .right-column-fix.floating').removeClass('pinned');$('#element .right-column-fix.floating .right-column-fix-inner').css({'margin-top':0});}
if(rfT+(rfH-riH)<=scrlTop+shO)
$('#element .right-column-fix.floating .right-column-fix-inner').addClass('bottom');else
$('#element .right-column-fix.floating .right-column-fix-inner').removeClass('bottom');}
if((($('.page-box.box-15 .box-list.box-list-rows').length==1)||($('.page-box.box-15 .box-list.box-list-rows-lead').length==1)||($('.page-box.box-15 .box-list.box-list-rows-large').length==1))&&($('.page-box.box-15 .box-list-rows-right .right-column-fix').length==1))
{var shO=0
shT=0,rfT=$('.page-box.box-15 .box-list-rows-right .right-column-fix.floating').offset().top,rfH=$('.page-box.box-15 .box-list-rows-right .right-column-fix.floating').outerHeight();riH=$('.page-box.box-15 .box-list-rows-right .right-column-fix.floating .right-column-fix-inner').outerHeight();if(($('#header').hasClass('pinned'))&&($('#header').is(':visible')))
shO=60;if(($('#element-header').hasClass('pinned'))&&($('#element-header').is(':visible')))
shO=120;shO=shO+20;if(rfT<=scrlTop+shO)
{$('.page-box.box-15 .box-list-rows-right .right-column-fix.floating').addClass('pinned');$('.page-box.box-15 .box-list-rows-right .right-column-fix.floating .right-column-fix-inner').css({'margin-top':shO});}
else
{$('.page-box.box-15 .box-list-rows-right .right-column-fix.floating').removeClass('pinned');$('.page-box.box-15 .box-list-rows-right .right-column-fix.floating .right-column-fix-inner').css({'margin-top':0});}
