ad_loadOptAd360();var embed_adpopup_script=document.createElement('script');embed_adpopup_script.id='embed_adpopup';embed_adpopup_script.type='text/javascript';embed_adpopup_script.async=true;embed_adpopup_script.src=embedDomain+'/embed/_js/adpopup.js';document.getElementsByTagName("head")[0].appendChild(embed_adpopup_script);});if(show_ads_outline)
{$('body').append('<style>'
+'.box-aside.ad-aside, '
+'.box-list.box-list li.box-list-item-ad .page-rectangle { overflow: unset; } '
+'.box-list.box-list li.box-list-item-ad .page-rectangle { margin-top: 15px; } '
