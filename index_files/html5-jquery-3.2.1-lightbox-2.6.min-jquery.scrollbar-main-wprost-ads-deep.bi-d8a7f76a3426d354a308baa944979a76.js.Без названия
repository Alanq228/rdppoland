{boxNextLiClass=$(boxNextLi).attr('class');boxNextLiClass=boxNextLiClass.replace('size-','old-size-');$(boxNextLi).attr('class',boxNextLiClass);$(boxNextLi).data('size','1p5x1');$(boxNextLi).data('curr-size','1p5x1');$(boxNextLi).addClass('size-1p5x1');$('.news-image img',boxNextLi).addClass('hidden');}}}},200);}}
if(($('#'+slotElement).parent('div').hasClass('page-billboard'))&&(event.isEmpty))
{if($('#'+slotElement).data('alcplse')!='true')
$('#'+slotElement).parent('div').addClass('hidden');if($('#'+slotElement).parents('div.page-box').length==1)
{var pb=$('#'+slotElement).parents('div.page-box');if($(pb).data('box')==1)
$(pb).addClass('hidden');}}
else
if(($('#'+slotElement).parent('div').hasClass('page-billboard'))&&(!event.isEmpty))
{if($('#'+slotElement).data('alcplse')!='true')
$('#'+slotElement).parent('div').removeClass('hidden');if($('#'+slotElement).parents('div.page-box').length==1)
{var pb=$('#'+slotElement).parents('div.page-box');if($(pb).data('box')==1)
$(pb).removeClass('hidden');}}
else
if(($('#'+slotElement).parent('div').hasClass('page-billboard-art'))&&(event.isEmpty))
{$('#'+slotElement).parent('div').addClass('hidden');}
else
if(($('#'+slotElement).parent('div').hasClass('page-billboard-comments'))&&(event.isEmpty))
{$('#'+slotElement).parent('div').addClass('hidden');}
else
if(($('#'+slotElement).parent('div').hasClass('page-rectangle-inner'))&&(event.isEmpty))
{$('#'+slotElement).parent('div.page-rectangle-inner').addClass('empty-rectangle');if($('#'+slotElement).parents('div.page-rectangle').length==1)
$('#'+slotElement).parents('div.page-rectangle').addClass('hidden');}
else
if(($('#'+slotElement).parent('div').hasClass('page-rectangle-inner'))&&(!event.isEmpty))
{$('#'+slotElement).parent('div.page-rectangle-inner').removeClass('empty-rectangle');if($('#'+slotElement).parents('div.page-rectangle').length==1)
$('#'+slotElement).parents('div.page-rectangle').removeClass('hidden');}
else
if(($('#'+slotElement).parent('div').hasClass('page-recommended'))&&(event.isEmpty))
{$('#'+slotElement).html('<a class="page-recommended-ad-wydanie" href="http://e.wprost.pl/" target="ewydanie" title="Tygodnik WPROST: e-wydanie"><img src="/_i/ad/ad-recommended-ewydanie.png" alt="Tygodnik WPROST: e-wydanie" /></a>')}
if($('#'+slotElement).parents('li.screen-ad').length==1)
{if(event.isEmpty)
{$('#gallery button.button.button-to-desc span').text('Wicej informacji');$('#gallery button.button.button-to-desc').removeClass('button-skip-ad');}
else
{if($('#'+slotElement).parents('div.page-rectangle').length==1)
{$('#'+slotElement).parents('div.page-rectangle').attr('data-slot',slotElement);$('#'+slotElement).parents('div.page-rectangle').css({'width':$('#'+slotElement).width(),'height':$('#'+slotElement).height(),'margin-left':(0-($('#'+slotElement).width()/2)),'margin-top':(0-($('#'+slotElement).height()/2)),});}}}
if(!event.isEmpty)
resizePage();}
function initDialog()
{$('body').append('<div id="dialog"><div class="wrapper"><div id="dialog-inner"></div></div></div>');$('#dialog').on('click',function(event){hideDialog();event.stopPropagation();return false;});$('#dialog .wrapper').on('click',function(event){event.stopPropagation();return false;});}
function showDialog()
{$('#dialog').addClass('visible');$('body').css('overflow','hidden');}
function hideDialog()
{$('#dialog').removeClass('visible');$('body').css('overflow','auto');setTimeout(function(){$('#dialog-inner').html('&nbsp;');},800);}
var headerMessageTimeout1,headerMessageTimeout2;function initHeaderMessage()
