function initCharts()
{if($('.art-chart').length>0)
{if($('#js_google_charts').length==0)
{var gchartsjs=document.createElement('script');gchartsjs.id='js_google_charts';gchartsjs.src='https://www.gstatic.com/charts/loader.js';gchartsjs.type='text/javascript';gchartsjs.async=true;if(gchartsjs.readyState)
{gchartsjs.onreadystatechange=function(){if(gchartsjs.readyState=='loaded'||gchartsjs.readyState=='complete')
drawGoogleCharts();};}
else
{gchartsjs.onload=function(){drawGoogleCharts();};}
document.getElementsByTagName("head")[0].appendChild(gchartsjs);}
else
drawGoogleCharts();}}
function drawGoogleCharts()
{google.charts.load('current',{'packages':['corechart']});if($('.art-chart').length>0)
{$('.art-chart').each(function(){var id=$(this).data('id'),type=$('#chart-'+id).data('type');if(id>0)
{$('#chart-'+id).append('<div class="wait"></div>');google.charts.setOnLoadCallback(function(){drawGoogleChart(id,type);});}});}}
function drawGoogleChart(id,type)
{var ds=JSON.parse($('#chart-ds-'+id).text()),options=ds.options,data=google.visualization.arrayToDataTable(ds.data);if(type=='bar')
{if($('#chart-'+id).data('orientation')==1)
var chart=new google.visualization.BarChart(document.getElementById('chart-'+id));else
var chart=new google.visualization.ColumnChart(document.getElementById('chart-'+id));}
else
if(type=='line')
var chart=new google.visualization.LineChart(document.getElementById('chart-'+id));else
if(type=='pie')
var chart=new google.visualization.PieChart(document.getElementById('chart-'+id));if(typeof chart=='object')
chart.draw(data,options);}
function initContests()
{if($('#js_contest').length==0)
{var cjs=document.createElement('script')
cjs.id='js_contest';cjs.src='/_js/contest.js';cjs.type='text/javascript';cjs.async=true;if(cjs.readyState)
{cjs.onreadystatechange=function(){if(cjs.readyState=='loaded'||cjs.readyState=='complete')
{runContests()}};}
else
{cjs.onload=function(){runContests()};}
document.getElementsByTagName("head")[0].appendChild(cjs);}
else
runContests()}
function initTabs()
{var ti=1,woffset=50;if($('.sub-header-tabs').length>0)
{$('.sub-header-tabs').each(function(){var lbi=$(this).parents('li.box-list-item-list').first()
tb=this,id=$(tb).attr('id'),sel=-1;if(!id)
{id='sub-header-tabs-'+ti;$(tb).attr('id',id);}
$('.sub-header',tb).each(function(){var i=0,w=$(this).width(),h=parseInt($(this).css('margin-top'),10)+parseInt($(this).css('margin-bottom'),10)+$(this).outerHeight();m=$('span',this).length-1;$('span',this).each(function(){$(this).addClass('pointer');$(this).addClass('sub-header-index-'+i);$(this).data('index',i);$(this).data('tab',id);$(this).data('left',(i*woffset));$(this).data('right',(w-(((m+1)-i)*woffset)));$(this).css({'left':$(this).data('right'),'right':'auto','opacity':1,'width':0,'z-index':i});if(($(this).hasClass('selected'))&&(sel<0))
sel=i;if($('#'+$(this).data('tabpage')).length==1)
{var lh=lbi.height()-(($('.sub-header-tabs-buttons',tb).length==1)?40:0);$('#'+$(this).data('tabpage')).addClass('sub-header-tabs-page');$('#'+$(this).data('tabpage')).data('right',w+10);$('#'+$(this).data('tabpage')).data('height',lh);$('#'+$(this).data('tabpage')).css({'position':'absolute','z-index':0,'min-height':lh,'height':lh,'top':h,'left':(($(this).data('index')==sel)?0:(w+10))});}
if(typeof $.fn.scrollbar=='function')
{$('#'+$(this).data('tabpage')).addClass('scrollbar-macosx');$('#'+$(this).data('tabpage')).css({'overflow':'hidden','overflow-y':'auto','margin':0});$('#'+$(this).data('tabpage')).scrollbar();$('#'+$(this).data('tabpage')).parent('.scroll-wrapper').height($('#'+$(this).data('tabpage')).data('height')-h);$('#'+$(this).data('tabpage')).parent('.scroll-wrapper').data('tabpage',$(this).data('tabpage'));$('#'+$(this).data('tabpage')).parent('.scroll-wrapper').attr('data-tabpage',$(this).data('tabpage'));$('#'+$(this).data('tabpage')).scrollbar();$('#'+$(this).data('tabpage')).parent('.scroll-wrapper').remove('.scroll-x');if($('.sub-header-tabs-buttons',tb).length==1)
$('#'+$(this).data('tabpage')).css({'margin-bottom':40});$('.scroll-element',$('#'+$(this).data('tabpage')).parent('.scroll-wrapper')).hide();$('#'+$(this).data('tabpage')).parent('.scroll-wrapper').animate({scrollTop:0},1);$('#'+$(this).data('tabpage')).animate({scrollTop:0},1);}
i++;});});if(sel<0)
sel=0;$('.sub-header span.pointer',tb).on('click',function(event){event.preventDefault();selectTab(this,true);event.stopPropagation();return false;});ti++;selectTab($('.sub-header span.pointer',tb).first(),false);setTimeout(function(){$('.sub-header span.pointer',tb).addClass('transition');$('.sub-header-tabs-page',tb).addClass('transition');$('.scroll-wrapper ul',tb).animate({scrollTop:0},1);$('.scroll-wrapper',tb).animate({scrollTop:0},1);},1500);$('.sub-header-tabs-buttons',tb).css({'opacity':1});});}}
function selectTab(sender,sendEvent)
{var tabs=$('#'+$(sender).data('tab')),selected=$(sender).data('index'),m=$('.sub-header span',tabs).length;$('.sub-header-tabs-buttons span',tabs).css({'opacity':0,'z-index':0});$('.scroll-element',tabs).hide();for(var i=0;i<m;i++)
{$('#'+$('span.sub-header-index-'+i,tabs).data('tabpage')).css({'z-index':0});$('span.sub-header-index-'+i,tabs).removeClass('selected');if(i<=selected)
{$('span.sub-header-index-'+i,tabs).css({'left':$('span.sub-header-index-'+i,tabs).data('left'),'width':0});if(i==selected)
{$('span.sub-header-index-'+i,tabs).addClass('selected');$('span.sub-header-index-'+i,tabs).css('width','auto');$('#'+$('span.sub-header-index-'+i,tabs).data('tabpage')).css({'z-index':99,'left':0});$('.sub-header-tabs-buttons span[data-tabpage="'+($('span.sub-header-index-'+i,tabs).data('tabpage'))+'"]',tabs).css({'opacity':1,'z-index':311});$('.scroll-wrapper[data-tabpage="'+($('span.sub-header-index-'+i,tabs).data('tabpage'))+'"] .scroll-element',tabs).show();}}
else
if(i>selected)
{$('span.sub-header-index-'+i,tabs).css({'left':$('span.sub-header-index-'+i,tabs).data('right'),'width':0});$('#'+$('span.sub-header-index-'+i,tabs).data('tabpage')).css({'z-index':99,'left':$('#'+$('span.sub-header-index-'+i,tabs).data('tabpage')).data('right')});}}
if(sendEvent)
{if((typeof gtag=='function')||(typeof GA_TRACKING_ID=='string'))
{gtag('event','SubHeaderTabs',{'event_category':'Interaction','event_label':'ChangeTab'});}
else
if(typeof ga=='function')
{ga('send',{hitType:'event',eventCategory:'Interaction',eventAction:'SubHeaderTabs',eventLabel:'ChangeTab'});}}}
function initSearch()
{if((typeof(Storage)!=='undefined')&&(localStorage.largeCursor==1))
$('body').addClass('large-cursor');if((typeof(Storage)!=='undefined')&&(localStorage.allBlur==1))
$('body').addClass('blur');if((typeof(Storage)!=='undefined')&&(localStorage.allSepia==1))
$('body').addClass('sepia');$('#header-search form').on('submit',function(){var sq=$.trim($('#q',this).val());if(sq=='large cursor')
{$('body').addClass('large-cursor');$('#q',this).val('');closeHeaderBars();if(typeof(Storage)!=='undefined')
localStorage.largeCursor=1;return false;}
else
if(sq=='cursor')
{$('body').removeClass('large-cursor');$('#q',this).val('');closeHeaderBars();if(typeof(Storage)!=='undefined')
localStorage.largeCursor=0;return false;}
else
if(sq=='brudne okulary')
{$('body').addClass('blur');$('#q',this).val('');closeHeaderBars();if(typeof(Storage)!=='undefined')
localStorage.allBlur=1;return false;}
else
if(sq=='czyste okulary')
{$('body').removeClass('blur');$('#q',this).val('');closeHeaderBars();if(typeof(Storage)!=='undefined')
localStorage.allBlur=0;return false;}
else
if(sq=='zrb stary')
{$('body').addClass('sepia');$('#q',this).val('');closeHeaderBars();if(typeof(Storage)!=='undefined')
localStorage.allSepia=1;return false;}
else
if(sq=='zrb nowy')
{$('body').removeClass('sepia');$('#q',this).val('');closeHeaderBars();if(typeof(Storage)!=='undefined')
localStorage.allSepia=0;return false;}});}
function arrowsExpandMore(sender,id_obj,url,pg,callback)
{$('body').css('cursor','progress');if(sender)
{$(sender).unbind('click');$(sender).fadeOut();}
$.ajax({type:'POST',url:url,data:{'pg':pg},dataType:'json',success:function(oJsonObject){$('body').css('cursor','wait');$('.wait',$('#'+id_obj)).remove();if(oJsonObject.items)
{$('#'+id_obj).append(oJsonObject.items);}
if(oJsonObject.arrows)
$('#'+id_obj+'-arrows').replaceWith(oJsonObject.arrows);else
$('#'+id_obj+'-arrows').html('');$('body').css('cursor','default');resizePage();rebuildBoxListLinks();detectImageLightness();if($('#'+id_obj).hasClass('box-list-tiles'))
{if(wall&&(typeof freewall=='function'))
{wall.reset(freewallOptions);wall.fitWidth();}
if(opinActive)
$('body').openPageInLayer('refreshDynamicLinks');}
if(callback&&(typeof callback=='function'))
{callback();}
if((typeof gtag=='function')||(typeof GA_TRACKING_ID=='string'))
{gtag('event','Pagination',{'event_category':'Interaction','event_label':'Expand','value':pg});if(oJsonObject.pageview=='send')
{gtag('config',GA_TRACKING_ID,{'page_location':location.protocol+'//'+location.host+url+((url.substr(-1)!='/')?'/':'')+pg,'page_path':oJsonObject.title,'page_title':url+((url.substr(-1)!='/')?'/':'')+pg+'?ref=expand',});}}
else
if(typeof ga=='function')
{ga('send',{hitType:'event',eventCategory:'Interaction',eventAction:'Pagination',eventLabel:'Expand',eventValue:pg});if(oJsonObject.pageview=='send')
{ga('send','pageview',{'location':location.protocol+'//'+location.host+url+((url.substr(-1)!='/')?'/':'')+pg,'title':oJsonObject.title,'page':url+((url.substr(-1)!='/')?'/':'')+pg+'?ref=autopg',});}}},beforeSend:function(){$('#'+id_obj+'-arrows .arrows-expand-bar-next').fadeOut(100);$('#'+id_obj+'-arrows').append('<div class="wait"></div>');},error:function(resultData){$('body').css('cursor','default');$('.wait',$('#'+id_obj)).remove();return false;}});}
