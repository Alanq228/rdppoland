function debBtnClick(sender,option)
{var lbi=$(sender).parents('li.box-list-item').first();if(option=='4author')
{if($('.news-titlelead-wrapper',lbi).hasClass('news-titlelead-wrapper-4author'))
{$('.news-titlelead-wrapper',lbi).removeClass('news-titlelead-wrapper-4author');$('.news-author',lbi).addClass('hidden');}
else
{$('.news-titlelead-wrapper',lbi).addClass('news-titlelead-wrapper-4author');$('.news-author',lbi).removeClass('hidden');}
if($('.news-titlelead-wrapper',lbi).hasClass('news-titlelead-wrapper-4author'))
$(sender).addClass('selected');else
$(sender).removeClass('selected');}
else
{if(lbi.hasClass('box-list-item-'+option))
lbi.removeClass('box-list-item-'+option);else
lbi.addClass('box-list-item-'+option);if(lbi.hasClass('box-list-item-'+option))
$(sender).addClass('selected');else
$(sender).removeClass('selected');}}
function str_wrap_letters(text,insert_char)
{let nowrap_after=['&','a','i','o','w','z','u','"a','"i','"o','"w','"z','"u','a','i','o','w','z','u','A','I','O','W','Z','U','"A','"I','"O','"W','"Z','"U','A','I','O','W','Z','U','do','od','bo','to','po','go','we','je','za','ze','e','i','im','a','na','ta','t','te','t','ma','my','by','wy','co','Do','Od','Bo','To','Ty','Ta','Te','Po','We','Za','e','A','Na','Ma','My','By','Wy','Co','Le','"Do','"Od','"Bo','"To','"Ta','"Te','"Po','"We','"Za','"e','"A','"Na','"Ma','"My','"By','"Wy','"Co','Do','Od','Bo','To','Ta','Te','Po','We','Za','e','A','Na','Ma','My','By','Wy','Co','aby','ale','dla','czy','nie','lub','pod','ni','za','oraz','Aby','Ale','Czy','Al.','Dla','and','or','of','is','in','as','us','if','for','the','And','Or','Of','Is','In','Im','As','For','The'];text=text.replace(/\r/g,'').replace(/\t/g,' ');for(let i=0;i<nowrap_after.length;i++)
{let tr=' '+nowrap_after[i]+' ';let re=new RegExp(tr,"gi");text=text.replace(re,' '+nowrap_after[i]+(insert_char?String.fromCharCode(8239):'&nbsp;'));}
return $.trim(text);}
function winCreateCookie(name,value,days)
{var parts=location.hostname.split('.'),subdomain=parts.shift(),domain=parts.slice(-2).join('.');if(days)
{var date=new Date();date.setTime(date.getTime()+(days*24*60*60*1000));var expires="; expires="+date.toGMTString();}
else
var expires="";document.cookie=name+"="+value+expires+"; path=/;domain=."+domain;return winReadCookie(name);}
function winReadCookie(name)
{var nameEQ=name+"=";var ca=document.cookie.split(';');for(var i=0;i<ca.length;i++)
{var c=ca[i];while(c.charAt(0)==' ')
c=c.substring(1,c.length);if(c.indexOf(nameEQ)==0)
return c.substring(nameEQ.length,c.length);}
return null;}
function winEraseCookie(name)
{winCreateCookie(name,'',-1);}
function initRODOCookiesPopup()
