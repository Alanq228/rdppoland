{return ad_slotRenderEnded(event);}
function ad_slotRenderEnded(event)
{var slotElement=event.slot.getSlotElementId();if((show_gpt_log==1)||(show_dfp_log==1))
console.log(event.slot.getSlotElementId(),event.slot.getAdUnitPath(),(!event.isEmpty),$('#'+slotElement).width()+'x'+$('#'+slotElement).height());$('#'+slotElement).addClass('rendered');if(($('#'+slotElement).parent('div').hasClass('page-fullpage'))&&(!event.isEmpty))
{var parentBoxLi=$('#'+slotElement).parents('li.box-list-item').first();if($(parentBoxLi).length==1)
{setTimeout(function(){$(parentBoxLi).addClass('size-1p5x2p5');$(parentBoxLi).show();if($(parentBoxLi).index()==0)
{var boxNextLi=$(parentBoxLi).next('li').first(),boxNextLiClass='';if($(boxNextLi).length==1)
{if(!$(boxNextLi).hasClass('size-1p5x1p5'))
{boxNextLiClass=$(boxNextLi).attr('class');boxNextLiClass=boxNextLiClass.replace('size-','old-size-');$(boxNextLi).attr('class',boxNextLiClass);$(boxNextLi).data('size','1p5x1p5');$(boxNextLi).data('curr-size','1p5x1p5');$(boxNextLi).addClass('size-1p5x1p5');$('.news-image img',boxNextLi).addClass('hidden');}
boxNextLi=$(boxNextLi).next('li').first();if(($(boxNextLi).length==1)&&(!$(boxNextLi).hasClass('size-1p5x1')))
{boxNextLiClass=$(boxNextLi).attr('class');boxNextLiClass=boxNextLiClass.replace('size-','old-size-');$(boxNextLi).attr('class',boxNextLiClass);$(boxNextLi).data('size','1p5x1');$(boxNextLi).data('curr-size','1p5x1');$(boxNextLi).addClass('size-1p5x1');$('.news-image img',boxNextLi).addClass('hidden');}}}},200);}}
else
if(($('#'+slotElement).parent('div').hasClass('page-fullpage'))&&(event.isEmpty))
{var parentBoxLi=$('#'+slotElement).parents('li.box-list-item').first();if($(parentBoxLi).length==1)
{setTimeout(function(){$(parentBoxLi).removeClass('size-1p5x2p5');$(parentBoxLi).hide();if($(parentBoxLi).index()==0)
{var boxNextLi=$(parentBoxLi).next('li').first(),boxNextLiClass='';if($(boxNextLi).length==1)
{if(!$(boxNextLi).hasClass('size-1p5x1'))
{boxNextLiClass=$(boxNextLi).attr('class');boxNextLiClass=boxNextLiClass.replace('size-','old-size-');$(boxNextLi).attr('class',boxNextLiClass);$(boxNextLi).data('size','1p5x1');$(boxNextLi).data('curr-size','1p5x1');$(boxNextLi).addClass('size-1p5x1');$('.news-image img',boxNextLi).addClass('hidden');}
boxNextLi=$(boxNextLi).next('li').first();if(($(boxNextLi).length==1)&&(!$(boxNextLi).hasClass('size-1p5x1')))
{boxNextLiClass=$(boxNextLi).attr('class');boxNextLiClass=boxNextLiClass.replace('size-','old-size-');$(boxNextLi).attr('class',boxNextLiClass);$(boxNextLi).data('size','1p5x1');$(boxNextLi).data('curr-size','1p5x1');$(boxNextLi).addClass('size-1p5x1');$('.news-image img',boxNextLi).addClass('hidden');}}}},200);}}
if(($('#'+slotElement).parent('div').hasClass('page-billboard'))&&(event.isEmpty))
{if($('#'+slotElement).data('alcplse')!='true')
$('#'+slotElement).parent('div').addClass('hidden');if($('#'+slotElement).parents('div.page-box').length==1)
{var pb=$('#'+slotElement).parents('div.page-box');if($(pb).data('box')==1)
$(pb).addClass('hidden');}}
else
if(($('#'+slotElement).parent('div').hasClass('page-billboard'))&&(!event.isEmpty))
{if($('#'+slotElement).data('alcplse')!='true')
$('#'+slotElement).parent('div').removeClass('hidden');if($('#'+slotElement).parents('div.page-box').length==1)
{var pb=$('#'+slotElement).parents('div.page-box');if($(pb).data('box')==1)
$(pb).removeClass('hidden');}}
else
if(($('#'+slotElement).parent('div').hasClass('page-billboard-art'))&&(event.isEmpty))
{$('#'+slotElement).parent('div').addClass('hidden');}
else
if(($('#'+slotElement).parent('div').hasClass('page-billboard-comments'))&&(event.isEmpty))
{$('#'+slotElement).parent('div').addClass('hidden');}
else
if(($('#'+slotElement).parent('div').hasClass('page-rectangle-inner'))&&(event.isEmpty))
{$('#'+slotElement).parent('div.page-rectangle-inner').addClass('empty-rectangle');if($('#'+slotElement).parents('div.page-rectangle').length==1)
$('#'+slotElement).parents('div.page-rectangle').addClass('hidden');}
else
