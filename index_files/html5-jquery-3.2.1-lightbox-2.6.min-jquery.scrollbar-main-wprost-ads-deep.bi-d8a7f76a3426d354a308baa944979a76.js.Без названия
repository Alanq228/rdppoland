function creatGenesisSlotID()
{var d=new Date(),tstmp=d.getTime();return'gen-ad-slot-'+tstmp+'-'+Math.floor(Math.random()*1000)+'-'+Math.floor(Math.random()*1000);}
function detectGenesisSlots()
{$('div[data-ad="genesis"]').each(function(){var device_width=((window.innerWidth>0)?window.innerWidth:screen.width),accept_device_width=true;if(show_log)
console.log('genesisAds internal','detectGenesisSlots',this);if($(this).data('device-min-width')>0)
{if(device_width<$(this).data('device-min-width'))
{showHideGenesisSlotParentArea(this,false);accept_device_width=false;}}
if($(this).data('device-max-width')>0)
{if(device_width>$(this).data('device-max-width'))
{showHideGenesisSlotParentArea(this,false);accept_device_width=false;}}
if(($(this).data('assigned')!=1)&&(accept_device_width))
{var id=$.trim($(this).attr('id')),type=$.trim($(this).attr('data-type')).toLowerCase(),slot=$.trim($(this).attr('data-slot')),size=$.trim($(this).attr('data-size')),msize=$.trim($(this).attr('data-multi-size')),exact_size=$.trim($(this).attr('data-size-exact')),can_refresh=$.trim($(this).attr('data-refresh')),release=$.trim($(this).attr('data-release')).toLowerCase(),ph_width=$(this).width(),width=0,height=0,sizes=[],min_width=0,temp_size=false,temp=false,_moneteasy=$.trim($(this).attr('data-moneteasy'));if(genAdMobile)
min_width=parseInt($(this).attr('data-mobile-size-min-width'));else
min_width=parseInt($(this).attr('data-desctop-size-min-width'));if(!min_width)
min_width=0;if(show_log)
console.log('genesisAds internal','detectGenesisSlots','SlotObjectData (before)','id: '+(typeof id),'type: '+type,'slot: '+slot,'size: '+size,'ph_width: '+ph_width,'min_width: '+min_width);if(!id)
{if(show_log)
console.log('genesisAds internal','detectGenesisSlots','Slot: new id');id=creatGenesisSlotID();$(this).attr('id',id);if(show_log)
console.log('genesisAds internal','detectGenesisSlots','Slot: id',id);}
if(size.indexOf('x')>0)
{temp_size=size.split('x');width=0+parseInt(temp_size[0]);height=0+parseInt(temp_size[1]);if(width<=ph_width)
{if((!min_width)||(width>=min_width))
sizes.push([width,height]);}
else
if(exact_size.toLowerCase()=='true')
{sizes.push([width,height]);}}
if(ph_width==0)
ph_width=width;if(msize.indexOf('x')>0)
{var w,h=0;if(msize.indexOf(',')>0)
{temp=msize.split(',');for(var i=0;i<temp.length;i++)
{temp_size=temp[i].split('x');if((temp_size.length==2)&&(!isInArray(temp_size,sizes)))
{w=parseInt($.trim(temp_size[0]));h=parseInt($.trim(temp_size[1]));if((w<=width)&&(w<=ph_width)&&(h<=height))
{if((!min_width)||(w>=min_width))
sizes.push([w,h]);}}}}
else
{temp_size=msize.split('x');if((temp_size.length==2)&&(!isInArray(temp_size,sizes)))
{w=parseInt($.trim(temp_size[0]));h=parseInt($.trim(temp_size[1]));if((w<=width)&&(w<=ph_width)&&(h<=height))
{if((!min_width)||(w>=min_width))
sizes.push([w,h]);}}}}
if(width==0)
width=300;if(height==0)
height=250;if(ph_width==0)
ph_width=width;if(show_log)
console.log('genesisAds internal','detectGenesisSlots','SlotObjectData (after)','id: '+id,'type: '+type,'slot: '+slot,'size: '+size,'sizes: '+sizes,'ph_width: '+ph_width,'min_width: '+min_width);if((release!='load')&&(release!='viewport'))
release='viewport';if((sessionStorage.gen_ads==1)&&(type.indexOf('doubleclick')>=0)&&(type.indexOf('genesis')>=0))
type='doubleclick';genAdSlots.push({'id':id,'sizes':sizes,'width':width,'height':height,'ph_width':ph_width,'can_refresh':(can_refresh.toLowerCase()=='true'),'type_genesis':(type.indexOf('genesis')>=0),'type_moneteasy':(type.indexOf('moneteasy')>=0),'type_optad360':((type.indexOf('optad360hb')>=0)||(type.indexOf('optad360')>=0)),'type_dfp':(type.indexOf('doubleclick')>=0),'release':release,'slot':(slot?slot:false),'moneteasy':(_moneteasy?_moneteasy:false),'dfp_slot':false,});$(this).css({'display':'inline-block','min-height':'1px','min-width':'1px'});if(type.indexOf('moneteasy')<0)
{$(this).css({'max-height':height,'max-width':width});}
else
{$(this).css({'display':'block'});}
$(this).attr('data-assigned',1);}});return genAdSlots;}
function runRefreshAdsInterval()
{var ref_slots=0;if(refreshAdsEnabled)
{if(typeof genAdSlots=='object')
{for(var slot in genAdSlots)
{if(((genAdSlots[slot].type_dfp)&&(!genAdSlots[slot].type_genesis))&&(typeof genAdSlots[slot].dfp_slot=='object')&&(genAdSlots[slot].dfp_slot!=tempDFPObject))
{if(genAdSlots[slot].can_refresh)
{ref_slots++;if(!genAdSlots[slot]['refresh-assigned'])
{genAdSlots[slot]['refresh-assigned']=true;$('#'+genAdSlots[slot].id).data('timer',0).attr('data-timer',0);$('#'+genAdSlots[slot].id).data('off-timer',-1).attr('data-off-timer',-1);$('#'+genAdSlots[slot].id).data('refresh-assigned','on').attr('data-refresh-assigned','on');if(show_refresh_blocks)
$('#'+genAdSlots[slot].id).after('<div id="ref_'+genAdSlots[slot].id+'">refresh: <i></i></div>');}}}}}
if((!refreshAdsInterval)&&(ref_slots>0))
{refreshAdsInterval=setInterval(function(){var slots2refresh=[]
for(var slot in genAdSlots)
{if((genAdSlots[slot].can_refresh)&&(typeof genAdSlots[slot].dfp_slot=='object')&&(genAdSlots[slot].dfp_slot!=tempDFPObject)&&(!$('#'+genAdSlots[slot].id).hasClass('not-rendered')))
{var curr_tm_pos=0+parseInt($('#'+genAdSlots[slot].id).data('timer'));off_tm_pos=0+parseInt($('#'+genAdSlots[slot].id).data('off-timer'));in_viewport=isElementInVerticalViewport(document.getElementById(genAdSlots[slot].id),-30);if(show_log)
console.log('genesisAds DFP','slots2refresh',genAdSlots[slot].id,curr_tm_pos+'/'+refreshAdsIntervalSecLimit,in_viewport);if(in_viewport)
{curr_tm_pos++;if((curr_tm_pos==refreshAdsIntervalSecLimit)||(off_tm_pos>refreshAdsIntervalOffSecLimit))
{slots2refresh.push(genAdSlots[slot].dfp_slot);curr_tm_pos=0;}
off_tm_pos=0;}
else
{curr_tm_pos=0;if(off_tm_pos>=0)
off_tm_pos++;}
$('#'+genAdSlots[slot].id).data('timer',curr_tm_pos).attr('data-timer',curr_tm_pos);$('#'+genAdSlots[slot].id).data('off-timer',off_tm_pos).attr('data-off-timer',off_tm_pos);if(show_refresh_blocks)
$('#ref_'+genAdSlots[slot].id+' i').text($('#'+genAdSlots[slot].id).data('timer')+' (off: '+$('#'+genAdSlots[slot].id).data('off-timer')+')');}}
if(show_log)
console.log('genesisAds DFP','slots2refresh',slots2refresh,slots2refresh.length);if(slots2refresh.length>0)
{if(googletag.pubads().isInitialLoadDisabled())
{googletag.pubads().refresh(slots2refresh,{changeCorrelator:false});}
else
googletag.pubads().refresh(slots2refresh);if(show_log)
console.log('genesisAds DFP','refresh',slots2refresh,slots2refresh.length);}},refreshAdsIntervalTime);}}}
function genesisAdsScrollPage()
{if((typeof genAdSlots=='object')&&(genAdSlots.length>0))
{for(var slot in genAdSlots)
{if((genAdSlots[slot].type_dfp)&&(!genAdSlots[slot].type_genesis)&&(!(!(document.getElementById(genAdSlots[slot].id)))))
{if(($('#'+genAdSlots[slot].id).hasClass('slot-dynamic'))&&($('#'+genAdSlots[slot].id).data('dfp-state')=='wait'))
{var in_viewport=isElementInVerticalViewport(document.getElementById(genAdSlots[slot].id));if((in_viewport)&&(typeof genAdSlots[slot].dfp_slot=='object')&&(genAdSlots[slot].dfp_slot==tempDFPObject))
{if(show_log)
console.log('genesisAds event','scrollPage','inViewport',genAdSlots[slot].id,in_viewport,$('#'+genAdSlots[slot].id).data('dfp-state'));var dfp_slot=ad_DFP_defineDisplaySlot(genAdSlots[slot].id,genAdSlots[slot].slot,genAdSlots[slot].sizes,false);if(typeof dfp_slot=='object')
{genAdSlots[slot].dfp_slot=dfp_slot;genAdSlots[slot].type_genesis=false;$('#'+genAdSlots[slot].id).attr('data-dfp-state','display').data('dfp-state','display');runRefreshAdsInterval();}
else
showHideGenesisSlotParentArea(genAdSlots[slot].id,false);}}}}}}
function showHideGenesisSlotParentArea(slot_obj,visible)
{if(show_log)
console.log('genesisAds event','showHideGenesisSlotParentArea',visible,slot_obj);if(typeof slot_obj=='string')
slot_obj=$('#'+slot_obj);if(typeof slot_obj!='object')
return false;if($(slot_obj).parents('div.optAdIns').first().hasClass('optAdIns'))
{$(slot_obj).parents('.optAdIns').first().addClass('gen-detect');if(visible)
$(slot_obj).parents('.optAdIns').first().addClass('rendered-opt');return false;}
if($(slot_obj).data('type')=='optad360')
return false;if($(slot_obj).data('type')=='optad360hb')
return false;if($(slot_obj).data('type')=='moneteasy')
return false;$(slot_obj).removeClass('rendered not-rendered');$(slot_obj).addClass(((!visible)?'not-':'')+'rendered');if($(slot_obj).data('callback'))
{var slot_obj_callback=$(slot_obj).data('callback');if(typeof window[slot_obj_callback]=='function')
window[slot_obj_callback](slot_obj,visible);}
if($(slot_obj).parents('aside').first().hasClass('box-aside'))
{if($('.page-rectangle',$(slot_obj).parents('aside').first()).length==2)
{}
else
{if(visible)
$(slot_obj).parents('aside').first().removeClass('hidden');else
$(slot_obj).parents('aside').first().addClass('hidden');}}
if($(slot_obj).parent('div').hasClass('page-billboard'))
{if($(slot_obj).data('alcplse')!='true')
{if(visible)
$(slot_obj).parent('div').removeClass('hidden');else
