{var v_up=parseInt(oJsonObject.count.up),v_down=parseInt(oJsonObject.count.down),v_can=parseInt(oJsonObject.can);displayVotesCount(v_up,v_down,v_can);}}});}}
function saveVote(vote)
{var id=getTargeData('id'),type=getTargeData('type');if((id>0)&&(type>0))
{$.ajax({type:'POST',url:'/hits.php',data:{'id':''+id,'type':''+type,'event':'vote_'+vote,'action':'set'},dataType:'json',success:function(oJsonObject){if(oJsonObject.count)
{var v_up=parseInt(oJsonObject.count.up),v_down=parseInt(oJsonObject.count.down),v_can=parseInt(oJsonObject.can);if(typeof(Storage)!=='undefined')
sessionStorage['vote_'+id+'_'+type]=vote;displayVotesCount(((vote=='up')?'inc':'const'),((vote=='down')?'inc':'const'),v_can);if((typeof gtag=='function')||(typeof GA_TRACKING_ID=='string'))
{gtag('event','Vote',{'event_category':'Interaction','event_label':''+vote,'event_value':id,});}
else
if(typeof ga=='function')
{ga('send',{hitType:'event',eventCategory:'Interaction',eventAction:'Vote',eventLabel:''+vote,eventValue:id});}}}});}}
function displayVotesCount(v_yes,v_no,can_vote)
{if(($('.art-details-votes').length==1)||($('.art-bar-buttons').length)||($('.profile-bar-buttons').length))
{if((v_yes=='inc'&&v_no=='const')||(v_yes=='const'&&v_no=='inc'))
{var vby=0+parseInt($('.vote-button-up').first().data('count')),vbn=0+parseInt($('.vote-button-down').first().data('count'));if(v_yes=='inc')
{v_yes=vby+1;v_no=vbn;}
else
if(v_no=='inc')
{v_yes=vby;v_no=vbn+1;}}
$('.art-details-votes').html('<span class="votes"><span class="vote-up">'+v_yes+'</span> <span class="vote-down">'+v_no+'</span></span>');var btns='<span class="vote-buttons"><span class="vote-button vote-button-up" data-count="'+v_yes+'"></span><span class="vote-button vote-button-down" data-count="'+v_no+'"></span></span>';if($('.art-bar-buttons').length>0)
{$('.art-bar-buttons .vote-buttons').remove();$('.art-bar-buttons').append(btns);}
if($('.profile-bar-buttons').length>0)
{$('.profile-bar-buttons .vote-buttons').remove();$('.profile-bar-buttons').append(btns);}
if(can_vote)
{$('.art-details-votes span.votes').prepend(btns);$('.art-details-votes span.votes').on('mouseover',function(){$('span.vote-buttons',this).css('opacity',1);});$('.art-details-votes span.vote-buttons').on('mouseleave',function(){$(this).css('opacity',0);});$('.vote-button-up').attr('title','Oce na TAK');$('.vote-button-up').on('click',function(){$('.vote-button-up').off('click',function(){return false;});$('.vote-button-down').off('click',function(){return false;});$('.art-details-votes vote-buttons').css('opacity','0');setTimeout(function(){saveVote('up');},200);});$('.vote-button-down').attr('title','Oce na NIE');$('.vote-button-down').on('click',function(){$('.vote-button-up').off('click',function(){return false;});$('.vote-button-down').off('click',function(){return false;});$('.art-details-votes vote-buttons').css('opacity','0');setTimeout(function(){saveVote('down');},200);});$('.vote-button').addClass('pointer');}
else
{var id=getTargeData('id'),type=getTargeData('type'),vy=$('.vote-button-up').first().data('count'),vn=$('.vote-button-down').first().data('count');$('.vote-button-up').attr('title',vy+' ocen'+((vy==1)?'a':'')+' na TAK');$('.vote-button-down').attr('title',vn+' ocen'+((vn==1)?'a':'')+' na NIE');if(typeof(Storage)!=='undefined')
{if(sessionStorage['vote_'+id+'_'+type]=='up')
$('.vote-button-down').addClass('gray');else
if(sessionStorage['vote_'+id+'_'+type]=='down')
$('.vote-button-up').addClass('gray');}}
$('.art-details-votes span.votes').addClass('visible');}}
function saveHCount()
{var id=getTargeData('id'),type=getTargeData('type'),hits_layout={};if((id>0)&&(type>0))
{sendH=true;hitsEvents['display_set']={'id':''+id,'type':''+type,'event':'display','action':'set','referer':(pageReferrer?atob(pageReferrer):document.referer),'referrer':(pageReferrer?atob(pageReferrer):document.referrer)};try
{if((typeof(Storage)!=='undefined')&&(sessionStorage['hits_layout']))
{hits_layout=JSON.parse(sessionStorage['hits_layout']);hits_layout.item=parseInt(hits_layout.item);hits_layout.id_box=parseInt(hits_layout.id_box);hits_layout.id_page=parseInt(hits_layout.id_page);hits_layout.id_page_box=parseInt(hits_layout.id_page_box);if((hits_layout.item>=0)&&(hits_layout.id_box>0)&&(hits_layout.id_page>0)&&(hits_layout.id_page_box>0))
hitsEvents['display_set']['layout']=hits_layout;sessionStorage['hits_layout']='';}}
catch(err)
{}}}
