function getArticleRelations()
{if($('.art-text.art-text-paywall').length==1)
return false;if($('#__relations').length==1)
{var rel_ids=$('#__relations').val(),presentations=false,videoCount=0,headerOption=$('.art-image').data('hdopt'),canAssignToMainImage=true,contests=false,products=false,quizzes=false,videos=false,charts=false;texts=false,files=false,posts=false,links=false;$.ajax({type:'POST',url:'/relations',data:{'relations':rel_ids},dataType:'json',success:function(oJsonObject){if(typeof oJsonObject=='object')
{rel_ids=rel_ids.replace('[','').replace(']','');rel_ids=rel_ids.split(',');for(var i=0;i<rel_ids.length;i++)
