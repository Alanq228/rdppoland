function updateMundial2018(mundial2018_live_element)
{$.ajax({type:'POST',url:'/mundial-2018/section',data:{'id':mundial2018_live_element},dataType:'json',success:function(mundial2018_info){if(typeof mundial2018_info=='object')
{if(typeof mundial2018_info.live_relation=='object')
{$('.page-box.page-box-mundial-2018-top table.live').html('<tr>'+'<td class="t t1'+(mundial2018_info.live_relation.state=='before'?'':' tr')+(mundial2018_info.live_relation.title_1_sp?' tsp':'')+'">'+mundial2018_info.live_relation.title_1+'</td>'+'<td class="i i1"><img src="'+mundial2018_info.live_relation.icon_1+'" /></td>'+
(mundial2018_info.live_relation.state=='before'?'':'<td class="r r1">'+mundial2018_info.live_relation.result_1+'</td>')+'<td class="m">:</td>'+
(mundial2018_info.live_relation.state=='before'?'':'<td class="r r2">'+mundial2018_info.live_relation.result_2+'</td>')+'<td class="i i2"><img src="'+mundial2018_info.live_relation.icon_2+'" /></td>'+'<td class="t t2'+(mundial2018_info.live_relation.state=='before'?'':' tr')+(mundial2018_info.live_relation.title_2_sp?' tsp':'')+'">'+mundial2018_info.live_relation.title_2+'</td>'+'</tr>');$('.page-box.page-box-mundial-2018-top div.live').html((mundial2018_info.live_relation.state=='before'?('<span>Replacja na ywo</span> <i>'+mundial2018_info.live_relation.date_start_day+' '+mundial2018_info.live_relation.date_start_time+'</i>'):(mundial2018_info.live_relation.state=='running'?'<span>Teraz na ywo</span>':'')));}}}});}
function exchangeRatesSlider()
{if(($('#page-biznes .exchange-rates-table').length>0)||(($('#page-main .exchange-rates-table').length>0)&&($('#page-box-1-87-11').length>0)))
{var link=document.createElement('link');link.rel='stylesheet';link.type='text/css';link.href='/_css/widgets/exchange-rates-slider.css';document.getElementsByTagName("head")[0].appendChild(link);setTimeout(function(){var excHTML='<div class="w"><ul>';excTable=$('.exchange-rates-table').first();$('tbody tr',excTable).each(function(){excHTML+='<li title="'+$(this).attr('title')+'" class="currency-item-'+$('.currency-code',this).text()+' status-'+$('.average',this).data('status')+' pointer">';excHTML+=$('.conversion',this).text()+' ';excHTML+='<b class="status-'+$('.average',this).data('status')+'">'+$('.average',this).text()+'</b>';excHTML+='</li>';});excHTML+='</ul></div>';if($('#page-main').length>0)
{$('#page-box-1-87-11 .page-box-header').after('<div id="exchange-rates-slider" class="in-page-box">'+excHTML+'</div>');if($('#page-box-1-339-11').length>0)
$('#page-box-1-339-11').remove();}
else
if($('#page-biznes').length>0)
{if($(window).width()<=660)
$('.page-header').after('<div id="exchange-rates-slider" class="under">'+excHTML+'</div>');else
$('.page-header').before('<div id="exchange-rates-slider">'+excHTML+'</div>');}
$('#exchange-rates-slider li').on('click',function(){let excr_url='https://biznes.wprost.pl/kursy-walut'
if($(this).hasClass('currency-item-USD'))
excr_url+='/USD';if($(this).hasClass('currency-item-CHF'))
excr_url+='/CHF';if($(this).hasClass('currency-item-GBP'))
excr_url+='/GBP';if($(this).hasClass('currency-item-EUR'))
excr_url+='/EUR';if($(this).hasClass('currency-item-JPY'))
excr_url+='/JPY';location=excr_url;});if(typeof $('#exchange-rates-slider ul').slick=='function')
exchangeRatesSliderRun();else
{var script=document.createElement('script')
script.src='/_js/jquery/jquery.slick.min.js';script.type='text/javascript';script.async=true;if(script.readyState)
{script.onreadystatechange=function(){if(script.readyState=='loaded'||script.readyState=='complete')
{script.onreadystatechange=null;exchangeRatesSliderRun();}};}
else
{script.onload=function(){exchangeRatesSliderRun();};}
document.getElementsByTagName("head")[0].appendChild(script);}},150);}}
function exchangeRatesSliderRun()
{if($('#exchange-rates-slider').hasClass('in-page-box'))
{$('#exchange-rates-slider ul').slick({autoplay:true,autoplaySpeed:2000,dots:false,arrows:false,infinite:true,speed:300,slidesToShow:6,centerMode:false,responsive:[{breakpoint:1195,settings:{slidesToShow:4}},{breakpoint:600,settings:{slidesToShow:3}},{breakpoint:420,settings:{slidesToShow:3}},{breakpoint:320,settings:{slidesToShow:2}}]});}
else
{$('#exchange-rates-slider ul').slick({autoplay:true,autoplaySpeed:2000,dots:false,arrows:false,infinite:true,speed:300,slidesToShow:6,centerMode:false,responsive:[{breakpoint:2000,settings:{slidesToShow:5}},{breakpoint:960,settings:{slidesToShow:4}},{breakpoint:580,settings:{slidesToShow:3}},{breakpoint:420,settings:{slidesToShow:3}},{breakpoint:320,settings:{slidesToShow:2}}]});}}
/* <root>/_js/ads.js */

var ads=true,genAdsConfig={dfp:true,optad360:false},embedDomain='https://embed.wprost.pl'
initAdIndex=0,initAdInterval=0,initJQIndex=0,initJQInterval=0,refreshAdsEnabled=true,refreshAdsInterval=0,refreshAdsIntervalTime=1000,refreshAdsIntervalSecLimit=0,refreshAdsIntervalOffSecLimit=0,tempDFPObject={'temp_dfp':1}
