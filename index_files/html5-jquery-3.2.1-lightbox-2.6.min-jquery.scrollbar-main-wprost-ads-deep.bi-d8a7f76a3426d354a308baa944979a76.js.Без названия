{$('#'+slotElement).parent('div.page-rectangle-inner').removeClass('empty-rectangle');if($('#'+slotElement).parents('div.page-rectangle').length==1)
$('#'+slotElement).parents('div.page-rectangle').removeClass('hidden');}
else
if(($('#'+slotElement).parent('div').hasClass('page-recommended'))&&(event.isEmpty))
{$('#'+slotElement).html('<a class="page-recommended-ad-wydanie" href="http://e.wprost.pl/" target="ewydanie" title="Tygodnik WPROST: e-wydanie"><img src="/_i/ad/ad-recommended-ewydanie.png" alt="Tygodnik WPROST: e-wydanie" /></a>')}
if(!event.isEmpty)
resizePage();}
