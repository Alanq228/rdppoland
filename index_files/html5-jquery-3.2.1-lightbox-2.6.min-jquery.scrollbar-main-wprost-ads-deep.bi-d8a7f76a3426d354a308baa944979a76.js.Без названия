contests=true;if(oJsonObject[rel_ids[i]].type=='presentation')
presentations=true;if(oJsonObject[rel_ids[i]].type=='product')
products=true;}
else
$('.art-text span[data-id="'+rel_ids[i]+'"]').remove();}
else
