function runGenesisAds()
{var iOptAd360=0;if(!is_slotRenderEnded)
{if(typeof window.googletag!='undefined')
{googletag.cmd.push(function(){googletag.pubads().addEventListener('slotRenderEnded',function(event){var slotElementId=event.slot.getSlotElementId(),slotElement=document.getElementById(slotElementId);if(show_log)
console.log('genesisAds event','slotRenderEnded',slotElementId+' = '+(!(!(slotElement))),(!event.isEmpty));if(!(!(slotElement)))
showHideGenesisSlotParentArea(slotElement,(!event.isEmpty));if(!event.isEmpty)
if(typeof resizePage=='function')
resizePage();});});}
is_slotRenderEnded=true;}
if(typeof detectGenesisSlots()=='object')
{for(var slot in genAdSlots)
{if((genAdSlots[slot].type_dfp)&&(!genAdSlots[slot].type_genesis))
{var dfp_slot=ad_DFP_CreateDynamicSlot(genAdSlots[slot].id,genAdSlots[slot].slot,genAdSlots[slot].sizes,false,genAdSlots[slot].release)
if(show_log)
console.log('genesisAds DFP','assign DFP',genAdSlots[slot].id,genAdSlots[slot].slot,(typeof dfp_slot=='object'),(dfp_slot))
if(typeof dfp_slot=='object')
genAdSlots[slot].dfp_slot=dfp_slot;else
showHideGenesisSlotParentArea(genAdSlots[slot].id,false);}
else
if(genAdSlots[slot].type_genesis)
{genEmbedAdSlots.push(genAdSlots[slot]);}
else
if(genAdSlots[slot].type_moneteasy)
{genMoneteasyAdSlots.push(genAdSlots[slot]);}
else
if(genAdSlots[slot].type_optad360)
{iOptAd360++;}}}
if(typeof afterRunGenesisAds=='function')
{afterRunGenesisAds();}
if(show_log)
console.log('genesisAds internal','genAdSlots',genAdSlots.length,genAdSlots)
initGenesisInternalAds();if((typeof genMoneteasyAdSlots=='object')&&(genMoneteasyAdSlots.length>0))
{genAdMoneteasyWidgets();}
if((typeof genAdSlots=='object')&&(genAdSlots.length>0))
{$(window).scroll(function(e){genesisAdsScrollPage();});}
runRefreshAdsInterval();$(document).ready(function(){if(genAdsConfig.optad360)
{if(iOptAd360==0)
{if(genAdsConfig.optad360_release=='always')
ad_loadOptAd360();}
else
