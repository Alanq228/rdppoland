function getArticleGalleries()
{var canAssignToMainImage=true,headerOption=$('.art-image').data('hdopt');if($('#__galleries').length==1)
{var rel_ids=$('#__galleries').val();$.ajax({type:'POST',url:'/relations',data:{'galleries':rel_ids},dataType:'json',success:function(oJsonObject){rel_ids=rel_ids.replace('[','').replace(']','');rel_ids=rel_ids.split(',');for(var i=0;i<rel_ids.length;i++)
{if(typeof oJsonObject[rel_ids[i]]=='object')
{if(oJsonObject[rel_ids[i]].html!='')
{if((headerOption==5)&&canAssignToMainImage)
{$('.art-image').addClass('art-image-with-gallery');$('.art-image').addClass('art-text-relation-gallery');$('.art-image .art-image-inner').html('<div class="art-text-relation-gallery">'+oJsonObject[rel_ids[i]].html+'</div>');$('.art-image .art-image-inner .media-desc').slideUp(function(){$('.art-image .art-image-inner .media-desc').remove();});$('.art-image .art-image-inner .art-text-relation-gallery').on('click',function(){if(typeof(Storage)!=='undefined')
sessionStorage.galleryBackUrl=location.href;});$('.art-text span[data-id="gallery'+rel_ids[i]+'"]').remove();canAssignToMainImage=false;}
else
{$('.art-text span[data-id="gallery'+rel_ids[i]+'"]').addClass('art-text-relation art-text-relation-gallery');$('.art-text span[data-id="gallery'+rel_ids[i]+'"]').html('<span class="icon"></span><div class="art-text-relation-inner">'+oJsonObject[rel_ids[i]].html+'</div>');if(!$('.art-text span[data-id="gallery'+rel_ids[i]+'"] .page-gallery').hasClass('gallery-lightbox'))
{$('.art-text span[data-id="gallery'+rel_ids[i]+'"] a').on('click',function(){if(typeof(Storage)!=='undefined')
sessionStorage.galleryBackUrl=location.href;})}}}
else
$('.art-text span[data-id="gallery'+rel_ids[i]+'"]').remove();}
else
$('.art-text span[data-id="gallery'+rel_ids[i]+'"]').remove();}}});}}
function initQuestionings()
{var q_rel_ids=[],qc_rel_ids=[],qo=0
qco=0;if($('span[data-special="true"][data-type="qcollections"]').length>0)
{$('span[data-special="true"][data-type="qcollections"]').each(function(){if(!$(this).hasClass('assigned'))
{var idq=$(this).data('id');qc_rel_ids.push(idq.replace(/qcollection/gi,''));}});}
if($('span[data-special="true"][data-type="questionings"]').length>0)
{$('span[data-special="true"][data-type="questionings"]').each(function(){if(!$(this).hasClass('assigned'))
{var idq=$(this).data('id');q_rel_ids.push(idq.replace(/questioning/gi,''));}});}
if((q_rel_ids.length>0)||(qc_rel_ids.length>0))
{$.ajax({type:'POST',url:'/relations',data:{'questionings':((q_rel_ids.length>0)?('['+q_rel_ids.join(',')+']'):0),'qcollections':((qc_rel_ids.length>0)?('['+qc_rel_ids.join(',')+']'):0)},dataType:'json',success:function(oJsonObject){for(var i=0;i<qc_rel_ids.length;i++)
{if((typeof oJsonObject['qcollections']=='object')&&(typeof oJsonObject['qcollections'][qc_rel_ids[i]]=='object'))
{if(oJsonObject['qcollections'][qc_rel_ids[i]].html!='')
{if($('#qcollection-'+qc_rel_ids[i]).length==0)
{qo++;qco++;$('span[data-id="qcollection'+qc_rel_ids[i]+'"]').first().addClass('assigned');if($('span[data-id="qcollection'+qc_rel_ids[i]+'"]').first().parents('div.art-text').length==1)
{$('span[data-id="qcollection'+qc_rel_ids[i]+'"]').first().addClass('art-text-relation art-text-relation-questionings');$('span[data-id="qcollection'+qc_rel_ids[i]+'"]').first().html('<span class="icon"></span><div class="art-text-relation-inner">'+oJsonObject['qcollections'][qc_rel_ids[i]].html+'</div>');}
else
{$('span[data-id="qcollection'+qc_rel_ids[i]+'"]').first().html(''+oJsonObject['qcollections'][qc_rel_ids[i]].html+'');}}}
else
$('span[data-id="qcollection'+qc_rel_ids[i]+'"]').remove();}
else
$('span[data-id="qcollection'+qc_rel_ids[i]+'"]').remove();}
for(var i=0;i<q_rel_ids.length;i++)
{if((typeof oJsonObject['questionings']=='object')&&(typeof oJsonObject['questionings'][q_rel_ids[i]]=='object'))
{if(oJsonObject['questionings'][q_rel_ids[i]].html!='')
{if($('#questioning-'+q_rel_ids[i]).length==0)
{qo++;$('span[data-id="questioning'+qc_rel_ids[i]+'"]').first().addClass('assigned');if($('span[data-id="questioning'+q_rel_ids[i]+'"]').first().parents('div.art-text').length==1)
{$('span[data-id="questioning'+q_rel_ids[i]+'"]').first().addClass('art-text-relation art-text-relation-questionings');$('span[data-id="questioning'+q_rel_ids[i]+'"]').first().html('<span class="icon"></span><div class="art-text-relation-inner">'+oJsonObject['questionings'][q_rel_ids[i]].html+'</div>');}
else
{$('span[data-id="questioning'+q_rel_ids[i]+'"]').first().html(''+oJsonObject['questionings'][q_rel_ids[i]].html+'');}}}
else
$('span[data-id="questioning'+q_rel_ids[i]+'"]').remove();}
else
$('span[data-id="questioning'+q_rel_ids[i]+'"]').remove();}
if(qo>0)
{var d=new Date(),n=''+d.getTime();n=n.substr(0,9);if($('#css_questioning').length==0)
$('body').append('<'+'link id="css_questioning" href="/_css/questioning.css?q='+n+'" rel="stylesheet" type="text/css" charset="utf-8" /'+'>');if($('#js_questioning').length==0)
$('body').append('<'+'script id="js_questioning" async src="/_js/questioning.js?q='+n+'" charset="utf-8"></'+'script>');else
{if(typeof initPageQuestionings!='function')
{setTimeout(function(){if(typeof initPageQuestionings=='function')
initPageQuestionings();},1000);}
else
initPageQuestionings();}}
if(qco>0)
{initQuestionings();}}});}}
function getThumbURI(id,width,height,callback)
{$.ajax({type:'GET',url:'/thumb/'+id+'/'+width+'/'+height,success:function(uri){if(callback&&(typeof callback=='function'))
callback(uri,id,width,height);}});}
function optimizeHTML(root)
{var html="",toClose='',openText=false,shortTags=' br hr ',blockTags=' div h2 h3 h4 h5 h6 p pre address ul ol li dl dt dd blockquote img table hr ',ELEMENT_NODE=1,DOCUMENT_FRAGMENT_NODE=11,TEXT_NODE=3,COMMENT_NODE=8;for(node=root.firstChild;node;node=node.nextSibling)
{switch(node.nodeType)
{case ELEMENT_NODE:case DOCUMENT_FRAGMENT_NODE:{let tag=node.tagName.toLowerCase();console.log('ELEMENT_NODE 1',tag,'openText',openText,'toClose',toClose,'blockTags',(blockTags.indexOf(' '+tag+' ')>=0),'Empty',($.trim(node.innerHTML)==''),node.innerHTML);if(tag=='header')
tag='p';if(openText&&(blockTags.indexOf(' '+tag+' ')>=0))
{html+="\n"+'</p>'+"\n";openText=false;if(toClose=='p')
toClose='';}
else
if(toClose!='')
{html+="\n"+'</'+toClose+'>'+"\n";toClose='';}
else
html+="\n";console.log('ELEMENT_NODE 2',tag,'openText',openText,'toClose',toClose);if((tag!='hr')&&(blockTags.indexOf(' '+tag+' ')>=0)&&($.trim(node.innerHTML)==''))
continue;if(((tag=='span')&&(node.getAttribute('data-special')!='true'))||((tag=='header')&&(node.getAttribute('class')=='entry-header'))||((tag=='div')&&(node.getAttribute('class')=='entry-content')))
{if(toClose!='')
{html+='</'+toClose+'>';toClose='';}
if(openText)
html+=' '+node.innerHTML;else
html+="\n"+'<p class="new">'+"\n"+$.trim(node.innerHTML)+' ';openText=true;}
else
{html+='<'+tag;let attrs=node.attributes;for(ai=0;ai<attrs.length;++ai)
{let a=attrs.item(ai),name=a.name.toLowerCase();html+=' '+name+'="'+a.value+'"';}
if(shortTags.indexOf(' '+tag+' ')>=0)
html+=' />'+"\n";else
{html+='>'+node.innerHTML;if(blockTags.indexOf(' '+tag+' ')>=0)
toClose=tag;else
{html+="\n"+'</'+tag+'>'+"\n";toClose='';}}}
break;}
case TEXT_NODE:{console.log('TEXT_NODE 1','openText',openText,'toClose',toClose);if($.trim(node.data)!='')
{if(toClose!='')
{html+='</'+toClose+'>';toClose='';}
if(openText)
html+=' '+node.data;else
html+="\n"+'<p class="new">'+"\n"+$.trim(node.data)+' ';openText=true;}
console.log('TEXT_NODE 2','openText',openText,'toClose',toClose,node.data);break;}
case COMMENT_NODE:{html+='<!--'+node.data+'-->';break;}}}
if(toClose!='')
html+='</'+toClose+'>';if(openText!='')
html+='</p>';console.log(html);return html;}
function issueMediaIcons(sender)
{if($(sender).hasClass('selected'))
$(sender).removeClass('selected');else
$(sender).addClass('selected');$('.button-media').css('opacity',0.3);$('.large-media-button').each(function(){var media=$(this).data('media');if($(this).hasClass('selected'))
$('.button-media-'+media).css('opacity',1);});}
function detectImageLightness()
{$('.box-list-tiles li.box-list-item').each(function(){var list_item=this;if(!$(list_item).hasClass('detected-lightness'))
{if($('.news-image img',list_item).length==1)
{var src=$('.news-image img',list_item).attr('src');if((src!='')&&(src!='/_i/pixel.png')&&(src!='undefined'))
{getImageLightness(list_item,src,function(brightness){});}}}})}
function getImageLightness(sender,imageSrc,callback)
{var colorSum=0;var img=document.createElement("img");img.src=imageSrc;img.style.display="none";document.body.appendChild(img);img.onload=function(){var canvas=document.createElement("canvas");canvas.width=this.width;canvas.height=this.height;var ctx=canvas.getContext("2d");ctx.drawImage(this,0,0);var imageData=ctx.getImageData(0,0,canvas.width,canvas.height);var data=imageData.data;var r,g,b,avg;for(var x=0,len=data.length;x<len;x+=4)
{r=data[x];g=data[x+1];b=data[x+2];avg=Math.floor((r+g+b)/3);colorSum+=avg;}
var brightness=Math.floor(colorSum/(this.width*this.height));callback(brightness);if(brightness<150)
$(sender).addClass('box-list-item-color-dark');else
$(sender).addClass('box-list-item-color-light');$(sender).addClass('detected-lightness');var removeElement=this;removeElement.parentNode.removeChild(removeElement);}}
function DFP_CreateDynamicSlotByIndex(index,type,placeholder,targeting)
{if((window.googletag&&googletag.apiReady)&&(typeof DFP_DynamicSlots=='object')&&(typeof DFP_DynamicSlots[index]=='object')&&(typeof DFP_DynamicSlots[index][type]=='object')&&(!(!(document.getElementById(placeholder)))))
{var sizeMapping=false;if(typeof window[DFP_DynamicSlots[index][type].sizeMapping]=='object')
sizeMapping=window[DFP_DynamicSlots[index][type].sizeMapping];return DFP_CreateDynamicSlot(DFP_DynamicSlots[index][type].adUnitPath,DFP_DynamicSlots[index][type].size,sizeMapping,placeholder);}
else
return false;}
function DFP_CreateDynamicSlot(adUnitPath,adSize,sizeMapping,placeholder,targeting)
{if((window.googletag&&googletag.apiReady)&&(typeof adUnitPath=='string')&&(typeof adSize=='object')&&(!(!(document.getElementById(placeholder)))))
{var dfp_slot=googletag.defineSlot(adUnitPath,adSize,placeholder);if(sizeMapping)
dfp_slot.defineSizeMapping(sizeMapping);dfp_slot.addService(googletag.pubads());googletag.display(placeholder);$('#'+placeholder).addClass('slot-dynamic');return dfp_slot;}
else
return false;}
