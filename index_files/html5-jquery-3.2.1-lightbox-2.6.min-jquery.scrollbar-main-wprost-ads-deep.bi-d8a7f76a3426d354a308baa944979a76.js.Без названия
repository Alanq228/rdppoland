function initShareButtons(share_container,share_url)
{if(!share_container)
share_container='.share-buttons';if((!isIE)&&(navigator.share)&&($('#header #share-button').length==0))
{var script=document.createElement('script')
script.type='text/javascript';script.async=true;script.src='/_js/share.js';document.getElementsByTagName("head")[0].appendChild(script);}
var btns='',_fb=$.trim($('meta[property="fb:app_id"]').attr('content')),_img=$.trim($('meta[property="og:image"]').attr('content')),_via=$.trim($('meta[property="twitter:site"]').attr('content')),_page=$.trim(encodeURIComponent($('#header .h1 a em').text())),_title=$.trim(encodeURIComponent($('meta[property="og:title"]').attr('content'))),_link=$.trim(encodeURIComponent($('meta[property="og:url"]').attr('content'))),_short=$.trim(encodeURIComponent($('link[rel="shortlink"]').attr('href')));if((!_short)||(_short=='undefined'))
_short=_link;if(!(!(share_url)))
{_short=share_url;_link=share_url;}
if($(share_container).length>0)
{btns+='<a class="share-button facebook-button" data-to="facebook" title="Udostpnij na Facebooku" href="https://www.facebook.com/share.php?u='+_link+'&t='+_title+'"  rel="nofollow"><i>Facebook</i></a>';btns+='<a class="share-button twitter-button" data-to="twitter" title="Udostpnij na Twitterze" href="https://twitter.com/share?url='+_short+'&text='+_title+((_via!='')?('&via='+_via.replace('@','')):'')+'"  rel="nofollow"><i>Twitter</i></a>';if(isMobile&&_fb)
btns+='<a class="share-button messenger-button" data-to="messenger" title="Udostpnij przez Messenger" href="fb-messenger://share?link='+_link+'&app_id='+encodeURIComponent(_fb)+'"  rel="nofollow"><i>Messenger</i></a>'
if(isMobile)
btns+='<a class="share-button whatsapp-button" data-to="whatsapp" title="Udostpnij przez Whatsapp" href="whatsapp://send?text='+_link+'" rel="nofollow"><i>Whatsapp</i></a>';btns+='<a class="share-button wykop-button" data-to="wykop" title="Udostpnij na Wykopie" href="https://www.wykop.pl/dodaj?url='+_link+'" rel="nofollow"><i>Wykop</i></a>';btns+='<a class="share-button linkedin-button" data-to="linkedin" title="Udostpnij na Linked In" href="https://www.linkedin.com/shareArticle?url='+_link+'&title='+_title+'"  rel="nofollow"><i>Linked In</i></a>';btns+='<a class="share-button tumblr-button" data-to="tumblr" title="Udostpnij na Tumblr" href="http://wwws.tumblr.com/share/link?url='+_link+'&name='+_title+'&description="  rel="nofollow"><i>Tumblr</i></a>';if(_img)
btns+='<a class="share-button pinterest-button" data-to="pinterest" title="Udostpnij na Pinterest" href="https://pinterest.com/pin/create/button/?url='+_link+'&media='+_img+'&description='+_title+'"  rel="nofollow"><i>Pinterest</i></a>'
btns+='<a class="share-button email-button" data-to="email" title="Wylij przez e-mail" href="mailto:?subject='+_title+'&amp;body='+encodeURIComponent('Odwied stron ')+_page+'%3A%0D%0A'+_link+'" rel="nofollow"><i>E-mail</i></a>'
$(share_container).append('<span class="inline-buttons">'+btns+'</span>');if(isMobile)
{if(_fb)
$('.inline-buttons',share_container).addClass('mob-4');else
$('.inline-buttons',share_container).addClass('mob-3');}
else
$('.inline-buttons',share_container).addClass('mob-2');$('a.share-button',share_container).on('click',function(event){saveSharedCount(this);if($(this).hasClass('email-button'))
{}
else
{window.open(this.href,'share-window');event.preventDefault();return false;}});if($(share_container).hasClass('content-share-buttons'))
{$(share_container+' .inline-buttons').append('<span class="share-button-more"></span>');$('span.share-button-more',share_container).on('click',function(event){$('.content-share-buttons .email-button').slideDown(30);$('.content-share-buttons .linkedin-button').slideDown(50);$('.content-share-buttons .tumblr-button').slideDown(100);$('.content-share-buttons .pinterest-button').slideDown(150);$('.content-share-buttons .wykop-button').slideDown(200);$(this).slideUp(function(){$(this).remove();});setTimeout(function(){setTimeout(function(){scrollPage();},2502);},2502);event.preventDefault();return false;});}
if($(share_container).parent('#share-buttons').length==1)
{$('#share-buttons '+share_container+' .share-button.facebook-button').addClass('long-button');$('#share-buttons '+share_container+' .share-button.twitter-button').addClass('long-button');$('#share-buttons '+share_container+' .share-button.whatsapp-button').addClass('long-button');$('#share-buttons '+share_container+' .share-button.messenger-button').addClass('long-button');$('#share-buttons '+share_container+' .share-button').not('.long-button').hide();$('#share-buttons '+share_container).append('<span class="share-button more-button"></span>');$('#share-buttons '+share_container+' .share-button.more-button').on('click',function(event){$(this).fadeOut(function(){$('#share-buttons '+share_container+' .share-button.long-button').removeClass('long-button');setTimeout(function(){$('#share-buttons '+share_container+' .share-button').show();},200);$(this).remove();});event.preventDefault();return false;});}}
