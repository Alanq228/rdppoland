function checkGenesisAd(ad)
{if(show_log)
console.log('genesisAds genesis','Check',ad.id);var today=new Date();today=today.toISOString().slice(0,10);if(ad.capping=='1_on_session')
{if(typeof(Storage)!=='undefined')
{if(sessionStorage.getItem('gen_ad_'+ad.id)==1)
{if(show_log)
console.log('genesisAds genesis','!1_on_session');return false;}}}
var lastTime=false;if(typeof(Storage)!=='undefined')
{lastTime=localStorage.getItem('gen_ad_'+ad.id);localStorage.getItem('gen_ad_'+ad.id);}
else
lastTime=winReadCookie('gen_ad_'+ad.id);if(!(!(lastTime)))
{if((ad.capping=='1_at_all')||((ad.capping=='1_on_day')&&(lastTime==today)))
{if(show_log)
console.log('genesisAds genesis','!capping');return false;}}
var rf=(pageReferrer?atob(pageReferrer):document.referrer);switch(ad.referrer)
{case'fb_tw':if((rf.indexOf('facebook.com')<0)&&((rf.indexOf('twitter.com')<0)&&(rf.indexOf('twitter.biz')<0)))
{if(show_log)
console.log('genesisAds genesis','!fb_tw');return false;}
break;case'facebook':if(rf.indexOf('facebook.com')<0)
{if(show_log)
console.log('genesisAds genesis','!facebook');return false;}
break;case'twitter':if((rf.indexOf('twitter.com')<0)&&(rf.indexOf('twitter.biz')<0))
{if(show_log)
console.log('genesisAds genesis','!twitter');return false;}
break;case'not_facebook':if(rf.indexOf('facebook.com')>=0)
{if(show_log)
console.log('genesisAds genesis','!!facebook');return false;}
break;case'not_twitter':if((rf.indexOf('twitter.com')>=0)||(rf.indexOf('twitter.biz')>=0))
{if(show_log)
console.log('genesisAds genesis','!!twitter');return false;}
break;}
return checkAllowedLocation(ad);}
function checkAllowedLocation(ad)
{var pages=ad.allow_pages,href=location.href,neg=[];if(ad.allow_pages.length>0)
{var allowed=false,this_href=location.protocol+'//'+location.hostname+location.pathname;for(var pi=0;pi<ad.allow_pages.length;pi++)
{var a=$.trim(ad.allow_pages[pi]),n=0;if(a=='*')
a='/*';if(a.substr(0,1)=='!')
{a=a.substr(1,a.length);n=1;}
if(a.substr(0,2)=='//')
a=location.protocol+a;else
if(a.substr(0,4)!='http')
a=location.protocol+'//'+location.hostname+a;ad.allow_pages[pi]=a;if(n==1)
neg.push(a);if(a.indexOf('*')>=0)
{var a1=a.substr(0,a.indexOf('*'))
a2=this_href.substr(0,a.indexOf('*'));if((a1==a2)&&(n==0))
{allowed=true;break;}}
else
if((ad.allow_pages[pi]==(location.protocol+'//'+location.hostname+location.pathname))&&(n==0))
{allowed=true;break;}
else
if((ad.allow_pages[pi]==(location.protocol+'//'+location.hostname+location.pathname))&&(n==1))
{break;}}
if(allowed)
{for(var pi=0;pi<ad.allow_pages.length;pi++)
{var a=$.trim(ad.allow_pages[pi]);if(a.substr(0,1)=='!')
{a=a.substr(1,a.length);neg.push(a);}}
if(neg.length>0)
{for(var ni=0;ni<neg.length;ni++)
{var a=$.trim(neg[ni]);if(a.substr(0,2)=='//')
a=location.protocol+a;else
if(a.substr(0,4)!='http')
a=location.protocol+'//'+location.hostname+a;if(a.indexOf('*')>=0)
{var a1=a.substr(0,a.indexOf('*'))
a2=this_href.substr(0,a.indexOf('*'));if(a1==a2)
{if(show_log)
console.log('neg',a1,a2);return false;}}
else
if(a==location.href)
{if(show_log)
console.log('neg',a);return false;}}}}
if(!allowed)
{if(show_log)
console.log('genesisAds genesis','!allow_pages',ad.allow_pages,neg);return false;}
return true;}
else
return false;}
function checkAllowedWidth(slot,ad)
{if(show_log)
console.log('genesisAds genesis','checkAllowedWidth'),genEmbedAdSlots[slot],ad;if((genEmbedAdSlots[slot].ph_width>=ad.width)&&(genEmbedAdSlots[slot].ph_width<(ad.width*2)))
{return true;}
else
if(typeof ad.alt_sizes=='object')
{for(var altIdent in ad.alt_sizes)
{if(genEmbedAdSlots[slot].ph_width>=ad.alt_sizes[altIdent].width)
{genEmbedAdSlots[slot].altIdent=altIdent;if(show_log)
console.log('genesisAds genesis','checkAllowedWidth','altIdent',altIdent,genEmbedAdSlots[slot],ad);return true;}}
if(show_log)
console.log('genesisAds genesis','!checkAllowedWidth');return false;}
else
{if(show_log)
console.log('genesisAds genesis','!checkAllowedWidth');return false;}}
function initGenesisInternalAds()
{if(show_log)
console.log('genesisAds genesis','initGenesisInternalAds',genEmbedAdSlots);var today=new Date();today=today.toISOString().slice(0,10);$.ajax({url:embedDomain+'/ad/displays',type:'POST',dataType:'json',success:function(oJsonObject){if(show_log)
console.log('genesisAds genesis','ajax',oJsonObject);if(typeof oJsonObject.displays=='object')
{if(show_log)
console.log('genesisAds genesis','displays',oJsonObject.displays);if((typeof genEmbedAdSlots=='object')&&(genEmbedAdSlots.length>0))
{var displays=[];for(var d in oJsonObject.displays)
{if(typeof oJsonObject.displays[d]=='object')
{if(checkGenesisAd(oJsonObject.displays[d]))
{if(show_log)
console.log('genesisAds genesis','checkedGenesisAd',oJsonObject.displays[d]);displays.push(oJsonObject.displays[d]);}}}
if(typeof displays=='object')
{if(show_log)
console.log('genesisAds genesis','checkedGenesisAds',displays);for(var d in displays)
{if((typeof displays[d]=='object')&&(typeof genEmbedAdSlots=='object'))
{var adDisplay=displays[d];if((adDisplay.slot!='')&&(document.getElementById((adDisplay.slot))))
{slotLoop:for(var slot in genEmbedAdSlots)
{if((genEmbedAdSlots[slot].id)&&(genEmbedAdSlots[slot].id==adDisplay.slot)&&(checkAllowedWidth(slot,adDisplay)))
{loadGenAdDisplay(adDisplay.slot,adDisplay,(genEmbedAdSlots[slot].altIdent?genEmbedAdSlots[slot].altIdent:false));if(typeof(Storage)!=='undefined')
{localStorage.setItem('gen_ad_'+adDisplay.id,today);sessionStorage.setItem('gen_ad_'+adDisplay.id,1);}
else
winCreateCookie('gen_ad_'+adDisplay.id,today,30);delete genEmbedAdSlots[slot];delete displays[d];break slotLoop;}}}}}
if(typeof displays=='object')
{for(var d in displays)
{if((typeof displays[d]=='object')&&(typeof genEmbedAdSlots=='object'))
{var adDisplay=displays[d];slotLoop:for(var slot in genEmbedAdSlots)
{if((genEmbedAdSlots[slot].id)&&(document.getElementById((genEmbedAdSlots[slot].id)))&&(checkAllowedWidth(slot,adDisplay)))
{loadGenAdDisplay(genEmbedAdSlots[slot].id,adDisplay,(genEmbedAdSlots[slot].altIdent?genEmbedAdSlots[slot].altIdent:false));if(typeof(Storage)!=='undefined')
{localStorage.setItem('gen_ad_'+adDisplay.id,today);sessionStorage.setItem('gen_ad_'+adDisplay.id,1);}
else
winCreateCookie('gen_ad_'+adDisplay.id,today,30);delete genEmbedAdSlots[slot];break slotLoop;}}}}}}
else
{if(show_log)
console.log('genesisAds genesis','!checkedGenesisAds');}}}
if(typeof oJsonObject.articles=='object')
{if(show_log)
console.log('genesisAds genesis','articles',oJsonObject.articles);if((($(window).width()>1192)&&($('#element .right-column #right-list').length==1))||(($(window).width()<1192)&&($('#element #middle-list').length==1)))
{var articles=[];for(var a in oJsonObject.articles)
{if(typeof oJsonObject.articles[a]=='object')
{if(checkGenesisAd(oJsonObject.articles[a]))
{var adArticle=oJsonObject.articles[a];loadGenAdArticle(adArticle);if(typeof(Storage)!=='undefined')
{localStorage.setItem('gen_ad_'+adArticle.id,today);sessionStorage.setItem('gen_ad_'+adArticle.id,1);}
else
winCreateCookie('gen_ad_'+adArticle.id,today,30);break;}
else
{if(show_log)
console.log('genesisAds genesis','!checkGenesisAd');}}}}}
if(typeof oJsonObject.stickies=='object')
{var adSticky=false;if(show_log)
console.log('genesisAds genesis','stickies',oJsonObject.stickies);for(var s in oJsonObject.stickies)
{if((typeof oJsonObject.stickies[s]=='object')&&(!adSticky))
{if(checkGenesisAd(oJsonObject.stickies[s]))
{adSticky=oJsonObject.stickies[s];loadGenAdSticky(adSticky);if(typeof(Storage)!=='undefined')
{localStorage.setItem('gen_ad_'+adSticky.id,today);sessionStorage.setItem('gen_ad_'+adSticky.id,1);}
else
winCreateCookie('gen_ad_'+adSticky.id,today,30);}
else
{if(show_log)
console.log('genesisAds genesis','!checkGenesisAd');}}}}
if(show_log)
console.log('genesisAds genesis','!ajax display',genEmbedAdSlots);for(var em_slot in genEmbedAdSlots)
{if((genEmbedAdSlots[em_slot].type_dfp)&&(genEmbedAdSlots[em_slot].id)&&(document.getElementById((genEmbedAdSlots[em_slot].id))))
{for(var slot in genAdSlots)
{if((genAdSlots[slot].id==genEmbedAdSlots[em_slot].id)&&(genAdSlots[slot].type_genesis))
{var dfp_slot=ad_DFP_CreateDynamicSlot(genAdSlots[slot].id,genAdSlots[slot].slot,genAdSlots[slot].sizes,false);if(typeof dfp_slot=='object')
{genAdSlots[slot].dfp_slot=dfp_slot;genAdSlots[slot].type_genesis=false;runRefreshAdsInterval();}
else
showHideGenesisSlotParentArea(genAdSlots[slot].id,false);}
else
if((genAdSlots[slot].id==genEmbedAdSlots[em_slot].id)&&(!genAdSlots[slot].type_genesis))
{showHideGenesisSlotParentArea(genAdSlots[slot].id,false);}}}}},error:function(event,request,settings){if(show_log)
console.log('genesisAds genesis','!ajax',event,request,settings);if((typeof genEmbedAdSlots=='object')&&(genEmbedAdSlots.length>0))
{for(var em_slot in genEmbedAdSlots)
{if((genEmbedAdSlots[em_slot].type_dfp)&&(genEmbedAdSlots[em_slot].id)&&(document.getElementById((genEmbedAdSlots[em_slot].id))))
{for(var slot in genAdSlots)
{if((genAdSlots[slot].id==genEmbedAdSlots[em_slot].id)&&(genAdSlots[slot].type_genesis))
{var dfp_slot=ad_DFP_CreateDynamicSlot(genAdSlots[slot].id,genAdSlots[slot].slot,genAdSlots[slot].sizes,false);if(typeof dfp_slot=='object')
{genAdSlots[slot].dfp_slot=dfp_slot;genAdSlots[slot].type_genesis=false;runRefreshAdsInterval();}
else
showHideGenesisSlotParentArea(genAdSlots[slot].id,false);}}}}}}});}
function fillUTMSource(url)
{if(url.indexOf('utm_source=&')>0)
{if(location.hostname.indexOf('pepsi')>=0)
url=url.replace(/utm_source\=/gi,'utm_source=PepsiEliot');else
if(location.hostname.indexOf('dorzeczy')>=0)
url=url.replace(/utm_source\=/gi,'utm_source=DoRzeczy.pl');else
if(location.hostname.indexOf('superhistoria')>=0)
url=url.replace(/utm_source\=/gi,'utm_source=superHISTORIA.pl');else
if(location.hostname.indexOf('zdrowie.wprost')>=0)
url=url.replace(/utm_source\=/gi,'utm_source=ZdrowieWprost.pl');else
if(location.hostname.indexOf('nieruchomosci.wprost')>=0)
url=url.replace(/utm_source\=/gi,'utm_source=NieruchomosciWprost.pl');else
if(location.hostname.indexOf('auto.wprost')>=0)
url=url.replace(/utm_source\=/gi,'utm_source=AutoWprost.pl');else
if(location.hostname.indexOf('info.wprost')>=0)
url=url.replace(/utm_source\=/gi,'utm_source=InfoWprost.pl');else
if(location.hostname.indexOf('biznes.wprost')>=0)
url=url.replace(/utm_source\=/gi,'utm_source=BixnesWprost.pl');else
if(location.hostname.indexOf('wellbeing.wprost')>=0)
url=url.replace(/utm_source\=/gi,'utm_source=WellbeingWprost.pl');else
if(location.hostname.indexOf('wprost')>=0)
url=url.replace(/utm_source\=/gi,'utm_source=Wprost.pl');else
if(location.hostname.indexOf('smaki.')>=0)
url=url.replace(/utm_source\=/gi,'utm_source=Smaki.pl');}
return url;}
function loadGenAdDisplay(placeholder,display,altIdent)
{if(show_log)
console.log('genesisAds genesis','Display','Load',placeholder,display);if(altIdent&&(typeof display.alt_sizes=='object'))
{display.image=display.alt_sizes[altIdent].image;display.width=display.alt_sizes[altIdent].width;display.height=display.alt_sizes[altIdent].height;}
display.url=fillUTMSource(display.url);$('#'+placeholder).html('<a href="'+display.url+'" class="_gen_ad_display" data-id="'+display.id+'" target="_display_'+display.id+'" rel="nofollow" '+'style="display:block; background:'+(display.color?display.color:'#F5F5F5')+'; width:'+display.width+'px; height:'+display.height+'px; outline:none; border:none; margin:0; padding:0;">'+'<img src="'+embedDomain+display.image+'" style="display:block; border:none; margin:0; padding:0;" />'+'</a>');$('#'+placeholder+' a._gen_ad_display').on('click',function(event){$.ajax({type:'POST',url:embedDomain+'/ad?r='+Math.floor((Math.random()*11)+1),data:{'id':''+$(this).data('id'),'action':'click'},success:function(){}});if((typeof gtag=='function')||(typeof GA_TRACKING_ID=='string'))
{gtag('event','Display',{'event_category':'Ad (Genesis)','event_label':'OpenLink','value':$(this).data('id')});}
else
if(typeof ga=='function')
{ga('send',{hitType:'event',eventCategory:'Ad',eventAction:'Display',eventLabel:'OpenLink',eventValue:$(this).data('id')});}
event.stopPropagation();});$.ajax({type:'POST',url:embedDomain+'/ad?r='+Math.floor((Math.random()*11)+1),data:{'id':''+display.id,'action':'display'}});}
function insertGenBoxListItem(base,method,id,article,notLazyLoad)
{if(base.indexOf(':last-child')>0)
{base=base.substr(0,base.indexOf(':last-child'));base=$(base).last();}
base=$(base);if($(base).length!=1)
return false;var box_list=$(base).parents('ul.box-list').first();if($(box_list).length!=1)
return false;var temp='_temp_box_list_item_'+id,temp_obj=false;$('body').append('<span style="display:none;" id="'+temp+'">'+$('li',box_list).first().html()+'</span>');if(article.image.substr(0,2)=='//')
article.image='https:'+article.image;$('.news-data-link',$('#'+temp)).attr('data-href',article.url).attr('data-element-id',article.id);$('.news-containers .item-containers',$('#'+temp)).html('<span class="cntr-commertial">'+article.label+'</span>'+((article.cntrs)?article.cntrs:''));$('a',$('#'+temp)).attr('rel',((article.rel)?article.rel:'nofollow')).attr('href',article.url).attr('title',article.title).attr('target','_article_'+article.id).attr('data-id',article.id);$('a.news-title strong span',$('#'+temp)).text(article.title);$('a.news-lead',$('#'+temp)).text(((article.lead)?article.lead:''));if(notLazyLoad)
{$('.news-image',$('#'+temp)).removeClass('news-image-lazyload').removeClass('news-image-lazyloaded').css({'background-color':article.color,'background-image':'url("'+article.image+'")'});$('.news-image img',$('#'+temp)).attr('alt',article.title).attr('data-src',article.image).attr('src',article.image);}
else
{$('.news-image',$('#'+temp)).addClass('news-image-lazyload').removeClass('news-image-lazyloaded').css({'background-color':article.color,'background-image':'url(/_i/pixel.png)'});$('.news-image img',$('#'+temp)).attr('alt',article.title).attr('data-src',article.image).attr('src','/_i/pixel.png');}
$('.news-image-gallery, .news-image-video, .news-image-quiz',$('#'+temp)).remove();var temp_item='<li id="'+id+'" class="'+$('li',box_list).first().attr('class')+((article.recommendation)?' box-list-item-recommendation ad-item box-list-item-recommendation-'+article.recommendation:'')+'">'+
$('#'+temp).html()+'</li>';if(method=='replace')
{temp_obj=$(temp_item).insertAfter(base);$(base).remove();$(temp_obj).show();}
else
{temp_obj=$(temp_item).insertAfter(base);$(temp_obj).slideDown();}
$('#'+temp).remove();return($(temp_obj).length==1);}
function loadGenAdArticle(article)
{if(show_log)
console.log('genesisAds genesis','Article','Load',article);article.url=fillUTMSource(article.url);article.recommendation='genesis-ads';var base_list=false;if(($(window).width()>1192)&&($('#element .right-column #right-list').length==1))
base_list='#element .right-column #right-list li:last-child';if(($(window).width()<1192)&&($('#element #middle-list').length==1))
base_list='#element #middle-list li:last-child';if(insertGenBoxListItem(base_list,'add','_gen_ad_article_'+article.id,article))
{$('#_gen_ad_article_'+article.id+' a').on('click',function(event){$.ajax({type:'POST',url:embedDomain+'/ad?r='+Math.floor((Math.random()*11)+1),data:{'id':''+$(this).data('id'),'action':'click'},success:function(){}});if((typeof gtag=='function')||(typeof GA_TRACKING_ID=='string'))
{gtag('event','Article Recommendation',{'event_category':'Ad (Genesis)','event_label':'OpenLink','value':$(this).data('id')});}
else
if(typeof ga=='function')
{ga('send',{hitType:'event',eventCategory:'Ad',eventAction:'Article Recommendation',eventLabel:'OpenLink',eventValue:$(this).data('id')});}
event.stopPropagation();});$.ajax({type:'POST',url:embedDomain+'/ad?r='+Math.floor((Math.random()*11)+1),data:{'id':''+article.id,'action':'display'}});}}
function loadGenAdSticky(sticky)
{}
function isInArray(s,a)
{if(a.length>0)
{for(var i=0;i<a.length;i++)
{if(a[i][0]==s[0]&&a[i][1]==s[1])
return true;}}
return false;}
/* <root>/_js/deep.bi.js */

function checkDeep()
{return(window.hasOwnProperty("deepTracker")&&window.deepTracker&&window.deepTracker.isInitialized())}
function callDeep(data)
{if(checkDeep()&&(typeof data=='object'))
{if(deep_service_ident&&(typeof deep_customer=='object'))
{data[deep_service_ident].customer=deep_customer;}
console.log('deep.bi','callDeep',data);deep(data);}}