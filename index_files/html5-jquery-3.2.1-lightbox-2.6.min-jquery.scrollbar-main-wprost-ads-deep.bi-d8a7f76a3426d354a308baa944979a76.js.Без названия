{var cite_text=$.trim(encodeURIComponent($(this).text()));$(this).addClass('binded');$(this).wrap('<a class="share-via-twitter" title="Udostpnij na Twitterze" href="http://twitter.com/share?url='+_short+'&text='+cite_text+((_via!='')?('&via='+_via.replace('@','')):'')+'"  rel="nofollow"></a>');}});$('.art-text a.share-via-twitter').each(function(){if(!$(this).hasClass('binded'))
