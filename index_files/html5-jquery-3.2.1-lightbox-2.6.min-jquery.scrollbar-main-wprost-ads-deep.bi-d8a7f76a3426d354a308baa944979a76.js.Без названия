function initHitsEvents()
{$.ajax({type:'POST',url:'/hits.php',data:{'hits_events':hitsEvents},dataType:'json',success:function(oJsonObject){for(var hitsEvent in hitsEvents)
{if(oJsonObject[hitsEvent]==0)
continue;switch(hitsEvent)
{case'display_set':var display_count=Math.abs(oJsonObject[hitsEvent].count);setTimeout(function(){if($('#element .art-details-comments').length==1)
{$('#element .art-details-comments').css('display','inline-block');$('#element .art-details-comments').addClass('visible');}},1000);if(display_count>0)
{setTimeout(function(){$('.art-details-display').html('<span class="display-hits"><span class="hits">'+display_count+'</span></span>');if(display_count<1000)
$('.art-details-display').addClass('hidden');if(display_count>2000)
$('.art-details-display .hits').addClass('hits-hot');if(display_count>5000)
$('.art-details-display .hits').addClass('hits-hot-red');$('.art-details-display .display-hits').addClass('visible');if(display_count>=1000)
{$('.header-art-display').html('<span>'+display_count+'</span>');$('.header-art-display').addClass('visible');}
else
$('.header-art-display').remove();},1200);}
break;case'share_get':var share_count=Math.abs(oJsonObject[hitsEvent].count);if(share_count>0)
{$('.share-button-icon').each(function(){if($('em',this).length==0)
$(this).append('<em>'+share_count+'</em>');else
$('em',this).text(share_count);$('em',this).removeClass('added');});$('.art-details-share').css('margin-right',$('.art-details-share .share-button-icon').width());}
$('.header-art-share').html(((share_count>0)?'<span>'+share_count+'</span>':'<span></span>'));$('.header-art-share').addClass('visible');break;case'vote_all_get':if(oJsonObject[hitsEvent].count)
{var v_up=parseInt(oJsonObject[hitsEvent].count.up),v_down=parseInt(oJsonObject[hitsEvent].count.down),v_can=parseInt(oJsonObject[hitsEvent].can);displayVotesCount(v_up,v_down,v_can);}
break;case'display_href_set':break;case'message_get':if(typeof oJsonObject[hitsEvent].message=='object')
{showHeaderMessage(oJsonObject[hitsEvent].message.text,oJsonObject[hitsEvent].message.url,oJsonObject[hitsEvent].message.type);}
break;}}},error:function(){}});}
