{$('.art-image').addClass('art-image-with-video');$('.art-image .art-image-inner').wrap('<div class="art-image-with-video-inner"></div>');$('.art-image .art-image-inner').css('background-image','url('+$('.art-image .art-image-inner img').attr('src')+')');$('.art-image .art-image-with-video-inner').prepend(oJsonObject[rel_ids[i]].html);$('.art-image .art-image-with-video-inner iframe').on('load',function(){$('.art-image .down').removeClass().addClass('play').click();});$('.art-text span[data-id="'+rel_ids[i]+'"]').remove();canAssignToMainImage=false;}
