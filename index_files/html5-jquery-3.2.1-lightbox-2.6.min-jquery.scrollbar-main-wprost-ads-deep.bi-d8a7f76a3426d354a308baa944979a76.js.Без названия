function _hashInt(str)
{var hash=0,i,chr;if(str.length==0)
return hash;for(i=0,l=str.length;i<l;i++)
{chr=str.charCodeAt(i);hash=((hash<<5)-hash)+chr;hash|=0;}
return Math.abs(hash);}
