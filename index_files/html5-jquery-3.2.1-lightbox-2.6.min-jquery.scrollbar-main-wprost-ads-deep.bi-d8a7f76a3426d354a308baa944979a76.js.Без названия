{return slotRenderEnded(event);}
function slotRenderEnded(event)
{var slotElement=event.slot.getSlotElementId(),show_gpt_log=(window.location.search.indexOf('show_gpt_log')>=0),show_dfp_log=(window.location.search.indexOf('show_dfp_log')>=0);if((show_gpt_log==1)||(show_dfp_log==1))
