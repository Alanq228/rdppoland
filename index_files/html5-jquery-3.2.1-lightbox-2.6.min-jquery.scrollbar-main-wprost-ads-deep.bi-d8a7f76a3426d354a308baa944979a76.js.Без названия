{$(this).parents('ul').first().addClass('assigned-cs');contentStreamDrawAd(this,resp_hash,response.ads[0]);return true;}});}
function contentStreamDrawAds(response)
{var resp_hash=response.placement.hash,profile_item=contentStreamProfiles[contentStreamProfile][contentStreamProfileMedia][contentStreamProfileType],profile_ph=false,profile_mt=false;if(typeof profile_item!='object')
return false;for(var x in profile_item)
{if((typeof profile_item[x].hash=='string')&&($.trim(profile_item[x].hash)==resp_hash))
{profile_ph=profile_item[x].ph,profile_mt=profile_item[x].mt;}}
if(!profile_ph)
return false;if((parseInt(response.ads.length)>0)&&(profile_ph.length<=response.ads.length))
{for(var i=0;i<response.ads.length;i++)
{if(i<profile_ph.length)
{var method='after';if(typeof(profile_mt)=='object')
{if(typeof(profile_mt[i])=='string')
method=profile_mt[i];}
contentStreamDrawAd($(profile_ph[i]),resp_hash,response.ads[i],method);}}}
else
